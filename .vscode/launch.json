{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "alp",
      "cwd": "${workspaceFolder}/alp",
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "args": [
        "--",
        "--ui5Uri https://ui5.sap.com --ui5 1.88.0",
        "--backendConfig",
        "[{\"path\":\"/sap\",\"destination\":\"abap-cloud-abap_adt\"}] "
      ],
      "windows": {
        "runtimeExecutable": "npm.cmd",
        "args": [
          "--",
          "--ui5Uri https://ui5.sap.com --ui5 1.88.0",
          "--backendConfig",
          "\"[{\\\"path\\\":\\\"/sap\\\",\\\"destination\\\":\\\"abap-cloud-abap_adt\\\"}]\" "
        ]
      },
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/alp\"}"
      }
    },
    {
      "name": "bhakar",
      "cwd": "${workspaceFolder}/bhakar",
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "args": [
        "--",
        "--ui5Uri https://ui5.sap.com --ui5 1.88.1",
        "--backendConfig",
        "[{\"path\":\"/sap\",\"destination\":\"abap-cloud-abap_adt\"}] "
      ],
      "windows": {
        "runtimeExecutable": "npm.cmd",
        "args": [
          "--",
          "--ui5Uri https://ui5.sap.com --ui5 1.88.1",
          "--backendConfig",
          "\"[{\\\"path\\\":\\\"/sap\\\",\\\"destination\\\":\\\"abap-cloud-abap_adt\\\"}]\" "
        ]
      },
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/bhakar\"}"
      }
    }
  ]
}