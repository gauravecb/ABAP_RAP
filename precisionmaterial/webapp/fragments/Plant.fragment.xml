<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:smartForm="sap.ui.comp.smartform"
	xmlns:smartField="sap.ui.comp.smartfield" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:f="sap.ui.layout.form">
	<smartForm:SmartForm id="idPlant" app:defaultCheckBoxDisplayBehaviour="OnOff" app:defaultDropDownDisplayBehaviour="idAndDescription"
		editable="true">
		<smartForm:layout>
			<smartForm:ColumnLayout columnsM="2" columnsL="2" columnsXL="2"/>
		</smartForm:layout>
		<smartForm:Group>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{MaterialFreightGroupsId}" editable="true" textLabel="Material Frieght Group"
						change="handleChangeMaterialFreightGroup">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{MaterialFreightGroupsDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField id="idCountryOfOrigin" value="{CountryOrgId}" editable="true" textLabel="Country of Origin"
						change="handleChangeCountryOfOrigin">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{CountryOrgDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{Post2InspStock}" editable="true" textLabel="Post to Inspection Stock" change="handleChangePostInspection">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text=""></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{GoldenTaxId}" editable="true" textLabel="Golden Tax" change="handleChangeGoldenTax">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{GoldenTaxDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{StorageConditionsId}" editable="true" textLabel="Storage Conditions" change="handleChangeStorageConditions">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{StorageConditionsDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
		</smartForm:Group>
	</smartForm:SmartForm>
</core:FragmentDefinition>