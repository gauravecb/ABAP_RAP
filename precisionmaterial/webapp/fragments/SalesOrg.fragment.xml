<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:smartForm="sap.ui.comp.smartform"
	xmlns:smartField="sap.ui.comp.smartfield" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:f="sap.ui.layout.form">
	<smartForm:SmartForm id="idSalesOrg" app:defaultCheckBoxDisplayBehaviour="OnOff" app:defaultDropDownDisplayBehaviour="idAndDescription"
		editable="true">
		<smartForm:layout>
			<smartForm:ColumnLayout columnsM="2" columnsL="2" columnsXL="2"/>
		</smartForm:layout>
		<smartForm:Group>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{SalesOrgId}" editable="true" textLabel="Sales Organization" change="handleSalesOrgChange">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{SalesOrgDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{DeliveryPlantId}" editable="true" textLabel="Delivery Plant" change="handleDeliveryPlantChange">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{DeliveryPlantDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{MinOrderQty}" textLabel="MOQ Customer">
						<!--<smartField:configuration>-->
						<!--	<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>-->
						<!--</smartField:configuration>-->
					</smartField:SmartField>
					<!--<Text text="{ProcessDesc}"></Text>-->
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{RoundProfileId}" editable="true" textLabel="Rounding Profile" change="handleChangeRoundingProfile">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{RoundProfileDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{ItemCategoryGroupsId}" editable="true" textLabel="Item Category Group" change="handleChangeItemCategory">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{ItemCategoryGroupsDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{MaterialGroupId}" editable="true" textLabel="Material Group" change="handleChangeMaterialGroup">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{MaterialGroupDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{PricingRefMat}" editable="true" textLabel="Pricing Ref Material" change="handleChangePricingRef">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
				</smartForm:elements>
				<Text text=""></Text>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{MaterialGroup1Id}" editable="true" textLabel="Material Group 1" change="handleChangeMaterialGroup1">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{MaterialGroup1Desc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{MaterialGroup2Id}" editable="true" textLabel="Material Group 2" change="handleChangeMaterialGroup2">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{MaterialGroup2Desc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{MaterialGroup3Id}" editable="true" textLabel="Material Group 3" change="handleChangeMaterialGroup3">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{MaterialGroup3Desc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{InnovationId}" editable="true" textLabel="Innovation Project">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{InnovationDescription}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{ProductAttribute2}" editable="true" textLabel="Product attribute 2">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false" controlType="checkbox"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{ProductAttribute3}" editable="true" textLabel="Product attribute 3">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false" controlType="checkbox"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{ProductAttribute4}" editable="true" textLabel="Product attribute 4">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false" controlType="checkbox"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
				</smartForm:elements>
			</smartForm:GroupElement>
		</smartForm:Group>
	</smartForm:SmartForm>
</core:FragmentDefinition>