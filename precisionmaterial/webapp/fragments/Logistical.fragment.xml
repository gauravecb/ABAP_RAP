<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:smartForm="sap.ui.comp.smartform"
	xmlns:smartField="sap.ui.comp.smartfield" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:f="sap.ui.layout.form">
	<App>
		<pages>
			<Page showHeader="false">
				<content>
					<smartForm:SmartForm id="idLogistical" app:defaultCheckBoxDisplayBehaviour="OnOff" app:defaultDropDownDisplayBehaviour="idAndDescription"
						editable="true">
						<smartForm:layout>
							<smartForm:ColumnLayout columnsM="2" columnsL="2" columnsXL="2"/>
						</smartForm:layout>
						<smartForm:Group>
							<smartForm:GroupElement>
								<smartForm:elements>
									<smartField:SmartField value="{SystemOfMeasure}" editable="true" textLabel="System of Measure">
										<smartField:configuration>
											<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
										</smartField:configuration>
									</smartField:SmartField>
									<Text text=""></Text>
								</smartForm:elements>
							</smartForm:GroupElement>
							<smartForm:GroupElement>
								<smartForm:elements>
									<smartField:SmartField value="{PieceCount}" editable="true" textLabel="Piece Count"/>
									<Text text=""></Text>
								</smartForm:elements>
							</smartForm:GroupElement>
							<smartForm:GroupElement>
								<smartForm:elements>
									<smartField:SmartField value="{NetWeight}" editable="true" textLabel="Net Weight"/>
									<Text text=""></Text>
								</smartForm:elements>
							</smartForm:GroupElement>
							<smartForm:GroupElement>
								<smartForm:elements>
									<smartField:SmartField value="{PrimaryTI}" editable="true" textLabel="Primary TI"/>
									<Text text=""></Text>
								</smartForm:elements>
							</smartForm:GroupElement>
							<smartForm:GroupElement>
								<smartForm:elements>
									<smartField:SmartField value="{PrimaryHI}" editable="true" textLabel="Primary HI"/>
									<Text text=""></Text>
								</smartForm:elements>
							</smartForm:GroupElement>
							<smartForm:GroupElement>
								<smartForm:elements>
									<smartField:SmartField value="{AlternateTI}" editable="true" textLabel="Alternate TI"/>
									<Text text=""></Text>
								</smartForm:elements>
							</smartForm:GroupElement>
							<smartForm:GroupElement>
								<smartForm:elements>
									<smartField:SmartField value="{AlternateHI}" editable="true" textLabel="Alternate HI"/>
									<Text text=""></Text>
								</smartForm:elements>
							</smartForm:GroupElement>
						</smartForm:Group>
					</smartForm:SmartForm>
					<Table id="idLogisticalTable" mode="SingleSelectLeft" items="{logisd}">
						<headerToolbar>
							<OverflowToolbar>
								<Title text="Logisticals Level"></Title>
								<ToolbarSpacer/>
								<Button icon="sap-icon://add" text="Add" press="handleAddLogisticsItem"></Button>
								<Button icon="sap-icon://delete" text="Delete" press="handleDeleteLogisticsItem"></Button>
							</OverflowToolbar>
						</headerToolbar>
						<columns>
							<Column>
								<header>
									<Text text="Units of Measure"></Text>
								</header>
							</Column>
							<Column>
								<header>
									<Text text="Denominator(X)"></Text>
								</header>
							</Column>
							<Column>
								<header>
									<Text text="Units(Y)"></Text>
								</header>
							</Column>
							<Column>
								<header>
									<Text text="Weight"></Text>
								</header>
							</Column>
							<Column>
								<header>
									<Text text="length"></Text>
								</header>
							</Column>
							<Column>
								<header>
									<Text text="Width"></Text>
								</header>
							</Column>
							<Column>
								<header>
									<Text text="Height"></Text>
								</header>
							</Column>
							<!--<Column>-->
							<!--	<header>-->
							<!--		<Text text="Volume"></Text>-->
							<!--	</header>-->
							<!--</Column>-->
							<Column>
								<header>
									<Text text="Barcode Required"></Text>
								</header>
							</Column>
							<Column>
								<header>
									<Text text="Barcode"></Text>
								</header>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<smartField:SmartField value="{LevelId}" editable="true" tooltip="{LevelDesc}" change="handleChangeLevelId">
										<smartField:configuration>
											<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
										</smartField:configuration>
									</smartField:SmartField>
									<smartField:SmartField value="{Denominator}" editable="true">
										<smartField:configuration>
											<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
										</smartField:configuration>
									</smartField:SmartField>
									<smartField:SmartField value="{Units}" editable="true">
										<smartField:configuration>
											<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
										</smartField:configuration>
									</smartField:SmartField>
									<smartField:SmartField value="{GrossWeight}" editable="true">
										<smartField:configuration>
											<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
										</smartField:configuration>
									</smartField:SmartField>
									<smartField:SmartField value="{Length}" editable="true">
										<smartField:configuration>
											<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
										</smartField:configuration>
									</smartField:SmartField>
									<smartField:SmartField value="{Width}" editable="true">
										<smartField:configuration>
											<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
										</smartField:configuration>
									</smartField:SmartField>
									<smartField:SmartField value="{Height}" editable="true">
										<smartField:configuration>
											<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
										</smartField:configuration>
									</smartField:SmartField>
									<smartField:SmartField value="{BarCodeRequired}" editable="true">
										<smartField:configuration>
											<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
										</smartField:configuration>
									</smartField:SmartField>
									<smartField:SmartField value="{BarCode}" editable="true">
										<smartField:configuration>
											<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
										</smartField:configuration>
									</smartField:SmartField>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</core:FragmentDefinition>