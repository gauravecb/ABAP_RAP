<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:smartForm="sap.ui.comp.smartform"
	xmlns:smartField="sap.ui.comp.smartfield" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:f="sap.ui.layout.form">
	<smartForm:SmartForm id="idPPP" app:defaultCheckBoxDisplayBehaviour="OnOff" app:defaultDropDownDisplayBehaviour="idAndDescription"
		editable="true">
		<smartForm:layout>
			<smartForm:ColumnLayout columnsM="2" columnsL="2" columnsXL="2"/>
		</smartForm:layout>
		<smartForm:Group>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{StorageLocationId}" editable="true" textLabel="Storage Location" change="handleStorageStockLocation">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{StorageLocationDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{LoadingGroupsId}" editable="true" textLabel="Loading Group" change="handleChangeLoadingGroup">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{LoadingGroupsDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{MaterialStatusId}" editable="true" textLabel="Material Status" change="handleChangeMaterialStatus">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{MaterialStatusDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{PlanningTimeFence}" editable="true" textLabel="Planning Time Fence">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text=""></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{LotSizeId}" editable="true" textLabel="Lot Size" change="handleChangeLotSize">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{LotSizeDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{ProdStorageLocationId}" editable="true" textLabel="Prod. Storage Location"
						change="handleChangeProdStorageLocation">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{ProdStorageLocationDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{EPStorageLocationId}" editable="true" textLabel="Eps. Storage Location"
						change="handleChangeEPSStorageLocation">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{EPStorageLocationDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{StockDeterminationGroupId}" editable="true" textLabel="Stock Determination"
						change="handleChangeStockDetermination">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{StockDeterminationGroupDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{StrategyGroupId}" editable="true" textLabel="Strategy Group" change="handleChangeStrategyGroup">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{StrategyGroupDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{AvailabilityCheckId}" editable="true" textLabel="Availability Check" change="handleChangeAvailability">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{AvailabilityCheckDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{TaxClassification}" editable="true" textLabel="Tax Classification">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text=""></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{InternalComment}" editable="true" textLabel="Internal Comment" change="handleChangePlanningBusiness">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text=""></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
		</smartForm:Group>
	</smartForm:SmartForm>
</core:FragmentDefinition>