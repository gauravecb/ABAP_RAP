<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:smartForm="sap.ui.comp.smartform"
	xmlns:smartField="sap.ui.comp.smartfield" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:f="sap.ui.layout.form">
	<smartForm:SmartForm id="idDemand" app:defaultCheckBoxDisplayBehaviour="OnOff" app:defaultDropDownDisplayBehaviour="idAndDescription"
		editable="true">
		<smartForm:layout>
			<smartForm:ColumnLayout columnsM="2" columnsL="2" columnsXL="2"/>
		</smartForm:layout>
		<smartForm:Group>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{VendorId}" editable="true" textLabel="Vendor Code" change="handleChangeVendorCode">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{VendorName}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{MinOrderQty}" editable="true" textLabel="Min Order Qty(Vendor)">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text=""></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{PurchaseLabourPrice}" editable="true" textLabel="Purchase/Labour Price (based on 1,000 units)"
						change="handleChangePlanningBusiness">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text=""></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{CurrencyCodesId}" editable="true" textLabel="Currency Code" change="handleChangeCurrencyCode">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text="{CurrencyCodesDesc}"></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartForm:elements>
					<smartField:SmartField value="{LeadTime}" editable="true" textLabel="Lead Time">
						<smartField:configuration>
							<smartField:Configuration preventInitialDataFetchInValueHelpDialog="false"></smartField:Configuration>
						</smartField:configuration>
					</smartField:SmartField>
					<Text text=""></Text>
				</smartForm:elements>
			</smartForm:GroupElement>
		</smartForm:Group>
	</smartForm:SmartForm>
</core:FragmentDefinition>