<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="false">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>Correspondence}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<f:SimpleForm class="customClassTitle editableForm" editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8"
			labelSpanS="6" adjustLabelSpan="false" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
			<f:content>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>DunnProcedure}" wrapping="true"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/DunngProcedure'},{path:'SearchDisplayModelSet>/results/DunngProcedureDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>LastDunned}" wrapping="true"/>
				<DatePicker enabled="false" displayFormat="dd.MM.YYYY" dateValue="{path:'SearchDisplayModelSet>/results/LastDunned'}"/>
				<Label textAlign="Left" text="{i18n>DunningClerk}" wrapping="true"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/DunningClerk'},{path:'SearchDisplayModelSet>/results/DunningClerkDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>DunningBlock}" wrapping="true"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/DunningBlock'},{path:'SearchDisplayModelSet>/results/DunningBlockDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>DunningLevel}" wrapping="true"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/results/DunningLevel}"/>
				<Label textAlign="Left" text="{i18n>DunningArea}" wrapping="true"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/DunningArea'},{path:'SearchDisplayModelSet>/results/DunningAreaDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>ClerkAbbrev}" wrapping="true"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/ClerkAbbrv'},{path:'SearchDisplayModelSet>/results/ClerkAbbrvDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>AcctAtCust}" wrapping="true"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/results/AcctAtCust}"/>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>CustomerUser}" wrapping="true"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/results/CustUser}"/>
				<Label textAlign="Left" text="{i18n>Clerk'sInternet}" wrapping="true"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/results/ClerkInternet}"/>
				<Label textAlign="Left" text="{i18n>AccountStatement}" wrapping="true"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/AccntStatemnt'},{path:'SearchDisplayModelSet>/results/AccntStatemntDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
			</f:content>
		</f:SimpleForm>
	</Panel>
</core:FragmentDefinition>