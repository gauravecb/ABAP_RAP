<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="false">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>BillingDocument}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<f:SimpleForm class="customClassTitle editableForm" editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8"
			labelSpanS="6" adjustLabelSpan="false" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
			<f:content>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>Rebate}" wrapping="true"/>
				<CheckBox selected="{path:'SearchDisplayModelSet>/results/Rebate',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}" enabled="false"/>
				<Label textAlign="Left" text="{i18n>PriceDetermination}" wrapping="true"/>
				<CheckBox enabled="false"
					selected="{path:'SearchDisplayModelSet>/results/PriceDetermn',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"/>
				<Label textAlign="Left" text="{i18n>InvoiceDates}" wrapping="true"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/InvoiceDates'},{path:'SearchDisplayModelSet>/results/InvoiceDatesDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>InvoiceListDates}" wrapping="true"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/InvoiceListDates'},{path:'SearchDisplayModelSet>/results/InvoiceListDatesDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>Incoterms1}" wrapping="true"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/Incoterms1'},{path:'SearchDisplayModelSet>/results/Incoterms1Desc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>Incoterms2}" wrapping="true"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/results/Incoterms2}"/>
				<Label textAlign="Left" text="{i18n>TermsofPayment}" wrapping="true"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/SalestermsOfPayment'},{path:'SearchDisplayModelSet>/results/SalestermsOfPaymentDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>AccountAssignmentGroup}" wrapping="true"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/AcctAssgmtGroup'},{path:'SearchDisplayModelSet>/results/AcctAssgmtGroupDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<core:Title/>
				<Link press="displayTaxClassificationDialog" text="{parts:[{path:'SearchDisplayModelSet>/TotalTaxClassications'},{path:'i18n>TaxCountryClassification'}],formatter:'MDMZCUSTOMER.model.Formatter.countBasedExtTypeValue'}"></Link>
			</f:content>
		</f:SimpleForm>
	</Panel>
</core:FragmentDefinition>