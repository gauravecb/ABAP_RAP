<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Dialog contentWidth="70%" title="{i18n>additionalIdentifiers} " class="sapUiSizeCompact">
		<Table enableBusyIndicator="false" items="{ExtIdentModelSet>/ExternalIdentifer/ExternalIdenitir}">
			<columns>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Text text="{i18n>ExternalIdentifierType}"/>
				</Column>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Text text="{i18n>ExternalIdentifierValue}"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<Text text="{ExtIdentModelSet>ExterIdtTypeDesc}"></Text>
					<Text text="{ExtIdentModelSet>ExtIdntValue}"></Text>
				</ColumnListItem>
			</items>
		</Table>
		<buttons>
			<Button text="{i18n>OK}" tooltip="{i18n>OK}" press="onPressExtIdentifierTypeValueFragOk"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>