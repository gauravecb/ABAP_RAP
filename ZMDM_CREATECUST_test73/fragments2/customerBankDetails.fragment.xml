<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="{PanelModelSet>/CustDataBankDetailsPanel}">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>CustomerBankDetails}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<VBox items="{BankDetailsModelSet>/BankDetails}">
			<f:SimpleForm visible="{BankDetailsModelSet>VisibleBasedOnNext}" editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8"
				labelSpanS="6" adjustLabelSpan="false" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
				<f:content>
					<core:Title/>
					<Label textAlign="Left" text="{i18n>IBAN}" wrapping="true"/>
					<Input enabled="false" value="{BankDetailsModelSet>IBAN}"/>
					<Label textAlign="Left" text="{i18n>country}" wrapping="true"/>
				<Input enabled="false"
						value="{BankDetailsModelSet>Countryname}"/>
					<Label textAlign="Left" text="{i18n>BankKey}" wrapping="true"/>
					<Input enabled="false" value="{BankDetailsModelSet>BankKey}"/>
					<Label textAlign="Left" text="{i18n>BankControlKey}" wrapping="true"/>
					<Input enabled="false" value="{BankDetailsModelSet>BankControlKey}"/>
					<core:Title/>
					<Label></Label>
					<Text></Text>
					<Label textAlign="Left" text="{i18n>BankAccount}" wrapping="true"/>
					<Input enabled="false" value="{BankDetailsModelSet>BankAccount}"/>
					<Label textAlign="Left" text="{i18n>BankAccountHolder}" wrapping="true"/>
					<Input enabled="false" value="{BankDetailsModelSet>BankAccountHolder}"/>
					<Label textAlign="Left" text="{i18n>BankType}" wrapping="true"/>
					<Input enabled="false" value="{BankDetailsModelSet>BankType}"/>
					<core:Title/>
					<Label></Label>
					<Text></Text>
					<Label textAlign="Left" text="{i18n>ReferenceDetails}" wrapping="true"/>
					<Input enabled="false" value="{BankDetailsModelSet>ReferenceDetails}"/>
					<Label textAlign="Left" text="{i18n>CollectionAuth}" wrapping="true"/>
					<Checkbox enabled="false" selected="{path:'BankDetailsModelSet>CollectionAuth',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"/>
					<Label></Label>
					<HBox>
						<Button tooltip="{i18n>PriviousBtn}" press="previosDipalyBankDetails" icon="sap-icon://slim-arrow-left"></Button>
						<Text text="{BankDetailsModelSet>AddCountNo}"></Text>
						<Button iconFirst="false" tooltip="{i18n>NextBtn}" press="nextDisplayBankDetails" icon="sap-icon://slim-arrow-right"></Button>
					</HBox>
				</f:content>
			</f:SimpleForm>
		</VBox>
		<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8" labelSpanS="6" adjustLabelSpan="false"
			emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
			<f:content>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>AlernativePayer}" wrapping="true"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/results/AlternativePayer}"/>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>IndividualEntries}" wrapping="true"/>
				<Checkbox enabled="false"
					selected="{path:'SearchDisplayModelSet>/results/IndividualEntries',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"/>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>AllowedPayer}" wrapping="true"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/results/AllowedPayer}"/>
			</f:content>
		</f:SimpleForm>
	</Panel>
</core:FragmentDefinition>