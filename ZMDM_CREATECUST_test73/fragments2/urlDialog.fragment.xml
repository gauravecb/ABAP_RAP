<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Dialog contentWidth="60%" title="{i18n>additionalURL} " class="sapUiSizeCompact">
		<Table  enableBusyIndicator="false" items="{SearchDisplayModelSet>/N_AddURL}">
			<columns>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Label text="{i18n>URL}"/>
				</Column>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Label text="{i18n>Comments}"/>
				</Column>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Label text="{i18n>Default}"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<Input liveChange="liveChangeURLFRag" value="{SearchDisplayModelSet>UrlHomepage}" enabled="false"></Input>
					<Input liveChange="liveChangeURLCommentFRag" value="{SearchDisplayModelSet>UrlComments}" enabled="false"></Input>
					<RadioButton enabled="false" select="selectGrpouDeualtUrlFrag" groupName="GroupURL" selected="{SearchDisplayModelSet>DefaultUrl}"/>
				</ColumnListItem>
			</items>
		</Table>
		<buttons>
			<Button text="{i18n>OK}" tooltip="{i18n>OK}" press="onPressURLValueFragOk"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>