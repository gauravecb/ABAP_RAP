<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="false">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>TaxData}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8" labelSpanS="6" adjustLabelSpan="false"
			emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
			<f:content>
				<core:Title/>
				<Label textAlign="Left" wrapping="true"
					text="{parts:[{path:'SearchDisplayModelSet>/TaxData/Text1'},{path:'SearchDisplayModelSet>/TaxData/NaturalPerson'},{path:'i18n>tax1'}],formatter:'MDMZCUSTOMER.model.Formatter.basedOnNaturalPersonChangeTaxLabel'}"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/TaxData/Tax_1Value}"/>
				<Label textAlign="Left" wrapping="true"
					text="{parts:[{path:'SearchDisplayModelSet>/TaxData/Text2'},{path:'SearchDisplayModelSet>/TaxData/NaturalPerson'},{path:'i18n>tax2'}],formatter:'MDMZCUSTOMER.model.Formatter.basedOnNaturalPersonChangeTaxLabel'}"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/TaxData/Tax_2Value}"/>
				<Label textAlign="Left" wrapping="true"
					text="{parts:[{path:'SearchDisplayModelSet>/TaxData/Text3'},{path:'SearchDisplayModelSet>/TaxData/NaturalPerson'},{path:'i18n>tax3'}],formatter:'MDMZCUSTOMER.model.Formatter.basedOnNaturalPersonChangeTaxLabel'}"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/TaxData/Tax_3Value}"/>
				<Label textAlign="Left" wrapping="true"
					text="{parts:[{path:'SearchDisplayModelSet>/TaxData/Text4'},{path:'SearchDisplayModelSet>/TaxData/NaturalPerson'},{path:'i18n>tax4'}],formatter:'MDMZCUSTOMER.model.Formatter.basedOnNaturalPersonChangeTaxLabel'}"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/TaxData/Tax_4Value}"/>
				<Label textAlign="Left" wrapping="true"
					text="{parts:[{path:'SearchDisplayModelSet>/TaxData/Text5'},{path:'SearchDisplayModelSet>/TaxData/NaturalPerson'},{path:'i18n>tax5'}],formatter:'MDMZCUSTOMER.model.Formatter.basedOnNaturalPersonChangeTaxLabel'}"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/TaxData/Tax_5Value}"/>
				<core:Title/>
				<Label text="{i18n>VatCountry}"/>
				<Input enabled="false" value="{VatDataModelSet>/TaxData/VatCountry}"/>
				<Label text="{i18n>VatNumber}"/>
				<Input enabled="false" value="{VatDataModelSet>/TaxData/VatNum}"/>
				<Label></Label>
				<Link press="onPressAddVatCountryVatNumberDisplay"
					text="{parts:[{path:'VatDataModelSet>/TaxData/TotalAdditionalVat'},{path:'i18n>labelCountVatCountryVatNum'}],formatter:'MDMZCUSTOMER.model.Formatter.countBasedVatCountryVatNum'}"></Link>
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8" labelSpanS="6" adjustLabelSpan="false"
			emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
			<f:content>
				<core:Title/>
				<Label text="{i18n>TaxJurisdictionCode}"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/TaxData/TaxJurisdictionCode}"/>
				<Label text="{i18n>TaxationType}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/TaxData/TaxationType'},{path:'SearchDisplayModelSet>/TaxData/TaxationTypeDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label text="{i18n>TaxNumberType}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/TaxData/TaxNumberType'},{path:'SearchDisplayModelSet>/TaxData/TaxNumberTypeDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<core:Title/>
				<Label text="{i18n>CFOPCategory}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/TaxData/CFOPCategory'},{path:'SearchDisplayModelSet>/TaxData/CfopCategoryDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label text="{i18n>ICMSLaw}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/TaxData/ICMSLaw'},{path:'SearchDisplayModelSet>/TaxData/IcmsLawDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label text="{i18n>IPI-Law}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/TaxData/IPILaw'},{path:'SearchDisplayModelSet>/TaxData/IpiLawDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<core:Title/>
				<Label text="{i18n>NaturalPerson}"/>
				<Checkbox enabled="false"
					selected="{path:'SearchDisplayModelSet>/TaxData/NaturalPerson',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"/>
				<Label text="{i18n>Sales/Pur.Tax}"/>
				<Checkbox enabled="false"
					selected="{path:'SearchDisplayModelSet>/TaxData/SalesPurTax',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"/>
				<Label text="{i18n>EqualizationTax}"/>
				<Checkbox enabled="false"
					selected="{path:'SearchDisplayModelSet>/TaxData/EqualizationTax',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"/>
				<Label text="{i18n>ICMS-Exempt}"/>
				<Checkbox enabled="false"
					selected="{path:'SearchDisplayModelSet>/TaxData/ICMSExempt',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"/>
				<Label text="{i18n>IPI-Exempt}"/>
				<Checkbox enabled="false"
					selected="{path:'SearchDisplayModelSet>/TaxData/IPIExempt',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"/>
			</f:content>
		</f:SimpleForm>
	</Panel>
</core:FragmentDefinition>