<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="false" visible="true">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>DeletionFlag}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<f:SimpleForm class="customClassTitle" editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8" labelSpanS="6"
			adjustLabelSpan="false" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
			<f:content>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>DeletionFlagGeneric}"/>
				<CheckBox enabled="false"
					selected="{path:'SearchDisplayModelSet>/results/DeletionFlagGeneric',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"/>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>DeletionFlagAccounting}"/>
				<CheckBox enabled="false"
					selected="{path:'SearchDisplayModelSet>/results/DeletionFlagAccounting',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"/>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>DeletionFlagSales}"/>
				<CheckBox enabled="false" value="{SearchDisplayModelSet>/results/DeletionFlagSales}"/>
				<core:Title/>
			</f:content>
		</f:SimpleForm>
	</Panel>
</core:FragmentDefinition>