<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="false">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>NameAddress}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<VBox items="{SearchDisplayNameAdressModelSet>/NameAndAddress}">
			<VBox>
				<f:SimpleForm visible="{SearchDisplayNameAdressModelSet>VisibleBasedOnNext}" editable="true" layout="ResponsiveGridLayout" labelSpanL="5"
					labelSpanM="5" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
					<f:content>
						<core:Title/>
						<Label text="{i18n>Country}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Country}"/>
						<Label text="{i18n>AddressVersion}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>AddressVersion}"/>
						<Label text="{i18n>PreferredAddressVersion}"></Label>
						<CheckBox enabled="false"
							selected="{path:'SearchDisplayNameAdressModelSet>PreferredAddressVersion', formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"/>
						<Label text="{i18n>Name1}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Name1}"/>
						<Label text="{i18n>Name2}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Name2}"/>
						<Label text="{i18n>Name3}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Name3}"/>
						<Label text="{i18n>Name4}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Name4}"/>
						<core:Title/>
						<Label text="{i18n>Street1}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Street1}"/>
						<Label text="{i18n>Street2}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Street2}"/>
						<Label text="{i18n>Street3}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Street3}"/>
						<Label text="{i18n>Street4}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Street4}"/>
						<Label text="{i18n>Street5}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Street5}"/>
						<Label text="{i18n>House}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>HouseNo}"/>
						<Label text="{i18n>HouseSupplement}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>HouseNoSupplement}"/>
						<Label text="{i18n>Building}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Building}"/>
						<Label text="{i18n>Floor}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Floor}"/>
						<Label text="{i18n>Room}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Room}"/>
						<core:Title/>
						<Label text="{i18n>PostalCode}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>PostalCode}"/>
						<Label text="{i18n>District}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>District}"/>
						<Label text="{i18n>City}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>City}"/>
						<Label text="{i18n>Region}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>Region}"/>
						<Label text="{i18n>FreeTradeRegion}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>FreeTradeRegion}"/>
						<Label text="{i18n>POBoxNumber}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>POBoxNumber}"/>
						<Label text="{i18n>POBoxPostalCode}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>POBoxPostalCode}"/>
						<Label text="{i18n>POBoxCity}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>POBoxCity}"/>
						<Label text="{i18n>CompanyPostalCode}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>CompanyPostalCode}"/>
					</f:content>
				</f:SimpleForm>
				<f:SimpleForm visible="{SearchDisplayNameAdressModelSet>VisibleBasedOnNext}" editable="true" layout="ResponsiveGridLayout" labelSpanL="5"
					labelSpanM="5" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
					<f:content>
						<core:Title/>
						<Label></Label>
						<Label text="{i18n>ByPassAddressValidation}"></Label>
						<CheckBox enabled="false" 
							selected="{path:'SearchDisplayNameAdressModelSet>ByPassAddressValidation',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"></CheckBox>
						<core:Title/>
						<Label text="{i18n>StreetValidation}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>StreetValidation}"/>
						<core:Title/>
						<Label text="{i18n>PoBoxValidation}"></Label>
						<Input enabled="false" value="{SearchDisplayNameAdressModelSet>PoBoxValidation}"/>
						<Label ></Label>
						<HBox>
							<Button tooltip="{i18n>PriviousBtn}" press="previosDispalyNameAndAddress" icon="sap-icon://slim-arrow-left"></Button>
							<Text text="{SearchDisplayNameAdressModelSet>AddCountNo}"></Text>
							<Button iconFirst="false" tooltip="{i18n>NextBtn}" press="nextDisplayNameAndAddress" icon="sap-icon://slim-arrow-right"></Button>
						</HBox>
					</f:content>
				</f:SimpleForm>
			</VBox>
		</VBox>
	</Panel>
</core:FragmentDefinition>