<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="false">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>SalesOrder}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<f:SimpleForm class="editableForm" editable="true" layout="ResponsiveGridLayout" labelSpanL="4" labelSpanM="8" labelSpanS="6"
			adjustLabelSpan="false" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
			<f:content>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>SalesDistrict}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/SalesDistrict'},{path:'SearchDisplayModelSet>/results/SalesDistrictDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>SalesOffice}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/SalesOffice'},{path:'SearchDisplayModelSet>/results/SalesOfficeDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>SalesGroup}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/SalesGroup'},{path:'SearchDisplayModelSet>/results/SalesGroupDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>CustomerGroup}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/CustomerGroup'},{path:'SearchDisplayModelSet>/results/CustomerGroupDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>Currency}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/Currency'},{path:'SearchDisplayModelSet>/results/CurrencyDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>AcctAtCust}"/>
				<Input enabled="false" value="{SearchDisplayModelSet>/results/SalesAcctAtCust}"/>
				<Label textAlign="Left" text="{i18n>ExchangeRateType}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/ExchRateType'},{path:'SearchDisplayModelSet>/results/ExchRateTypeDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>PriceGroup}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/PriceGrp'},{path:'SearchDisplayModelSet>/results/PriceGrpDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>CustPricProc}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/CustPricProc'},{path:'SearchDisplayModelSet>/results/CustPricProcDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>PriceList}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/PriceList'},{path:'SearchDisplayModelSet>/results/PriceListDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>CustStatGrp}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/CustStatsGrp'},{path:'SearchDisplayModelSet>/results/CustStatsGrpDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
			</f:content>
		</f:SimpleForm>
	</Panel>
</core:FragmentDefinition>