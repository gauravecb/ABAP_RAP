<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Dialog contentWidth="70%" title="{i18n>additionalTelephone} " class="sapUiSizeCompact">
		<Table  enableBusyIndicator="false" items="{SearchDisplayModelSet>/N_AddTelep}">
			<columns>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Label text="{i18n>Telephone}"/>
				</Column>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Label text="{i18n>Extension}"/>
				</Column>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Label text="{i18n>Comments}"/>
				</Column>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Label text="{i18n>Default}"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<Input liveChange="liveChangeTelephoneFragTable" value="{SearchDisplayModelSet>Telephone}" enabled="false"></Input>
					<Input liveChange="liveChangeExtenstionFragTable" value="{SearchDisplayModelSet>TelephoneExt}" enabled="false"></Input>
					<Input liveChange="liveChangeTeleponeCommentFragTable" value="{SearchDisplayModelSet>TeleComments}" enabled="false"></Input>
					<RadioButton select="selectDefaultTelephoneDailog" groupName="GroupTelephone" selected="{SearchDisplayModelSet>DefaultTele}" enabled="false"/>
				</ColumnListItem>
			</items>
		</Table>
		<buttons>
			<Button text="{i18n>OK}" tooltip="{i18n>OK}" press="onPressTelephoneValueFragOk"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>