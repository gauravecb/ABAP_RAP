<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Dialog contentWidth="70%" title="{i18n>TaxClassifications} " class="sapUiSizeCompact">
		<Table enableBusyIndicator="false" items="{SearchDisplayModelSet>/N_TaxClassif}">
			<columns>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Text text="{i18n>Country}"/>
				</Column>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Text text="{i18n>CountryName}"/>
				</Column>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Text text="{i18n>TaxCategory}"/>
				</Column>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Text text="{i18n>Name}"/>
				</Column>
				<Column demandPopin="true" minScreenWidth="Tablet">
					<Text text="{i18n>TaxClassification}"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<Input value="{SearchDisplayModelSet>Aland}" enabled="false"></Input>
					<Input value="{SearchDisplayModelSet>Landx}" enabled="false"></Input>
					<Input value="{SearchDisplayModelSet>Tatyp}" enabled="false"></Input>
					<Input value="{SearchDisplayModelSet>Vtxtk}" enabled="false"></Input>
				<Input value="{parts:[{path:'SearchDisplayModelSet>Taxkd'},{path:'SearchDisplayModelSet>Vtext'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"
	enabled="false"></Input>
				</ColumnListItem>
			</items>
		</Table>
		<buttons>
			<Button  text="{i18n>OK}" tooltip="{i18n>OK}" press="onPressTaxClassificationOkFrag"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>