<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="false">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>BlockData}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<f:SimpleForm class="customClassTitle" editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8" labelSpanS="6"
			adjustLabelSpan="false" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
			<f:content>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>PostingBlock}"/>
				<CheckBox enabled="false"
					selected="{path:'SearchDisplayModelSet>/results/PostingBlock',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"/>
				<Label textAlign="Left" text="{i18n>OrderBlock}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/OrderBlock'},{path:'SearchDisplayModelSet>/results/OrderBlockDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>DeliveryBlock }"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/DeliveryBlock'},{path:'SearchDisplayModelSet>/results/DeliveryBlockDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				<Label textAlign="Left" text="{i18n>BillingBlock}"/>
				<Input enabled="false"
					value="{parts:[{path:'SearchDisplayModelSet>/results/BillingBlock'},{path:'SearchDisplayModelSet>/results/BillingBlockDesc'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
	
				<!--<Label textAlign="Left" text="{i18n>OrderBlock}"/>-->
				<!--<Input enabled="false"-->
				<!--	value="{SearchDisplayModelSet>/results/OrderBlockDesc}"/>-->
				<!--<Label textAlign="Left" text="{i18n>DeliveryBlock }"/>-->
				<!--<Input enabled="false"-->
				<!--	value="{SearchDisplayModelSet>/results/DeliveryBlockDesc}"/>-->
				<!--<Label textAlign="Left" text="{i18n>BillingBlock}"/>-->
				<!--<Input enabled="false"-->
				<!--	value="{SearchDisplayModelSet>/results/BillingBlockDesc}"/>-->
				
				<Label textAlign="Left" text="{i18n>BlockSalesSupport}"/>
				<CheckBox selected="{path:'SearchDisplayModelSet>/results/BlockSalesSupport',formatter:'MDMZCUSTOMER.model.Formatter.checkBoxCheck'}"
					enabled="false"/>
				<core:Title/>
				<core:Title/>
			</f:content>
		</f:SimpleForm>
	</Panel>
</core:FragmentDefinition>