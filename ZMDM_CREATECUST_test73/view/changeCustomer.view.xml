<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="MDMZCUSTOMER.controller.changeCustomer"
	xmlns:ux="sap.uxap" xmlns:fb="sap.ui.comp.filterbar" xmlns:f="sap.ui.layout.form" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:objPage="sap.uxap">
	<!--	<App>
		<pages>-->
	<Page title="{i18n>changeCustomerPageTitle}" class="sapUiSizeCompact" showNavButton="{ButtonPropertyModel>/ShowNav}"
		navButtonPress="onPressChnageToSearch">
		<content>
			<objPage:ObjectPageLayout id="ObjectPageLayout" preserveHeaderStateOnScroll="false" alwaysShowContentHeader="false"
				navigate="onChnageIconTabBar" showTitleInHeaderContent="false" showEditHeaderButton="false" editHeaderButtonPress="handleEditBtnPress"
				upperCaseAnchorBar="false" useIconTabBar="true" headerContentPinnable="false">
				<objPage:headerContent>
					<Text id="ID_NOTE_HDR_TXT" visible="{ButtonPropertyModel>/hdrTextVisible}"
						text="Please select the Company Code and/or Sales Area and the Account Group"/>
					<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="5" emptySpanL="0" emptySpanM="0" columnsL="3"
						columnsM="3">
						<core:Title></core:Title>
						<Label text="{i18n>CompanyCode}"
							required="{path:'RequiredOptionalSet>/CompanyCodeCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"></Label>
						<Input showValueStateMessage="false" valueState="{HeaderModelSet>/CompnyCodeKeyValState}" change="onSelectCompanyCode"
							placeholder="{i18n>select} {i18n>CompanyCode}"
							enabled="{parts:[{path:'HeaderModelSet>/CompnyCodeKeyEnable'},{path:'RequiredOptionalSet>/CompanyCodeCond'}],formatter:'MDMZCUSTOMER.model.Formatter.EnabledHdrBusAreaBothCond'}"
							value="{HeaderModelSet>/CompnyCodeKey}"></Input>
						<core:Title></core:Title>
						<Label text="{i18n>SalesOrg}" required="{path:'RequiredOptionalSet>/SalesOrgCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"></Label>
						<Input tooltip="{i18n>tooltipSalesArea}" showValueStateMessage="false" change="onChangeSalesOrgSelected"
							placeholder="{i18n>select} {i18n>SalesOrg}" valueState="{HeaderModelSet>/SalesOrgKeyValState}"
							enabled="{parts:[{path:'HeaderModelSet>/SalesOrgKeyEnable'},{path:'RequiredOptionalSet>/SalesOrgCond'}],formatter:'MDMZCUSTOMER.model.Formatter.EnabledHdrBusAreaBothCond'}"
							value="{HeaderModelSet>/SalesOrgKey}"></Input>
						<Label text="{i18n>DistributionChannel}"
							required="{path:'RequiredOptionalSet>/DistriChanlCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"></Label>
						<Input showValueStateMessage="false" valueState="{HeaderModelSet>/DistribChnnlKeyValState}" change="onSelectDistributionChannel"
							placeholder="{i18n>select} {i18n>DistributionChannel}"
							enabled="{parts:[{path:'HeaderModelSet>/DistribChnnlKeyEnable'},{path:'RequiredOptionalSet>/DistriChanlCond'}],formatter:'MDMZCUSTOMER.model.Formatter.EnabledHdrBusAreaBothCond'}"
							value="{HeaderModelSet>/DistribChnnlKey}"></Input>
						<Label text="{i18n>Division}" required="{path:'RequiredOptionalSet>/DivisionCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"></Label>
						<Input showValueStateMessage="false" valueState="{HeaderModelSet>/DivisionKeyValState}" change="onSelectDivision"
							placeholder="{i18n>select} {i18n>Division}" value="{HeaderModelSet>/DivisionKey}"
							enabled="{parts:[{path:'HeaderModelSet>/DivisionKeyEnable'},{path:'RequiredOptionalSet>/DivisionCond'}],formatter:'MDMZCUSTOMER.model.Formatter.EnabledHdrBusAreaBothCond'}"></Input>
						<core:Title></core:Title>
						<!--	enabled="{parts:[{path:'HeaderModelSet>/SystemEnable'},{path:'RequiredOptionalSet>/SystemCond'}],formatter:'MDMZCUSTOMER.model.Formatter.EnabledHdrBusAreaBothCond'}"
						-->
						<Label text="{i18n>System}" required="{path:'RequiredOptionalSet>/SystemCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"></Label>
						<Input showValueStateMessage="false" valueState="{HeaderModelSet>/SystemValState}" change="liveChangeSystem"
							value="{HeaderModelSet>/System}" enabled="{HeaderModelSet>/SystemEnable}" textAlign="Left" placeholder="{i18n>enter} {i18n>System}"></Input>
						<Label text="{i18n>AccountGroup}"
							required="{path:'RequiredOptionalSet>/AccountGrpCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"></Label>
						<Input tooltip="{i18n>tooltipAccountGroup}" valueState="{HeaderModelSet>/AccountGrpKeyValState}"
							enabled="{parts:[{path:'HeaderModelSet>/AccountGrpKeyEnable'},{path:'RequiredOptionalSet>/AccountGrpCond'}],formatter:'MDMZCUSTOMER.model.Formatter.EnabledHdrBusAreaBothCond'}"
							showValueStateMessage="false" value="{HeaderModelSet>/AccountGrpKey} - {HeaderModelSet>/AccountGrpDesc}" change="onAccountGroupSelected"
							placeholder="{i18n>select}" visible="{ButtonPropertyModel>/AccountGroupCB}"></Input>
						<Label text="{i18n>AccountGroup}"
							required="{path:'RequiredOptionalSet>/AccountGrpCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"></Label>
						<Input value="{path:'HeaderModelSet>/AccountGrpKey',formatter:'MDMZCUSTOMER.model.Formatter.AccountGroupText'}" enabled="false"
							visible="{ButtonPropertyModel>/AccountGroupInput}"></Input>
						<Label text="{i18n>ApplicationId}"
							required="{path:'RequiredOptionalSet>/ApplicIdCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"></Label>
						<!--				enabled="{parts:[{path:'HeaderModelSet>/AppIdEnableEnable'},{path:'RequiredOptionalSet>/ApplicIdCond'}],formatter:'MDMZCUSTOMER.model.Formatter.EnabledHdrBusAreaBothCond'}"-->
						<Input liveChange="liveChangeAppId" showValueStateMessage="false" valueState="{HeaderModelSet>/AppIdValState}"
							value="{HeaderModelSet>/AppId}" enabled="false"></Input>
						<Label text="{i18n>goldenRecordIdChange}"></Label>
						<Input enabled="false" value="{HeaderModelSet>/GoldenRecordId}"></Input>
						<f:content>
							<core:Title/>
						</f:content>
					</f:SimpleForm>
				</objPage:headerContent>
				<objPage:sections >
					<objPage:ObjectPageSection id="goalsSection" title="{i18n>BusinessPartnerData}">
						<objPage:subSections>
							<objPage:ObjectPageSubSection id="goalsSectionSS1">
								<objPage:blocks>
									<core:Fragment type="XML" fragmentName="MDMZCUSTOMER.fragments.businessPartnerData"></core:Fragment>
								</objPage:blocks>
							</objPage:ObjectPageSubSection>
						</objPage:subSections>
					</objPage:ObjectPageSection>
					<objPage:ObjectPageSection title="{i18n>CustomerData}" id="ID_OP_CUST_DATA">
						<objPage:subSections >
							<objPage:ObjectPageSubSection >
								<objPage:blocks>
									<core:Fragment type="XML" fragmentName="MDMZCUSTOMER.fragments.customerData"></core:Fragment>
								</objPage:blocks>
							</objPage:ObjectPageSubSection>
						</objPage:subSections>
					</objPage:ObjectPageSection>
					<objPage:ObjectPageSection title="{i18n>ContactDetails}" id="ID_OP_CON_DET">
						<objPage:subSections>
							<objPage:ObjectPageSubSection>
								<objPage:blocks>
									<core:Fragment type="XML" fragmentName="MDMZCUSTOMER.fragments.contactDetails"></core:Fragment>
								</objPage:blocks>
							</objPage:ObjectPageSubSection>
						</objPage:subSections>
					</objPage:ObjectPageSection>
					<objPage:ObjectPageSection title="{i18n>AccountingData}" visible="{AccountingDataModelSet>/AccountingDataTab/AccountingDataTabVisible}"
						id="ID_OP_ACCOUNT_DATA">
						<objPage:subSections>
							<objPage:ObjectPageSubSection>
								<objPage:blocks>
									<core:Fragment type="XML" fragmentName="MDMZCUSTOMER.fragments.accountingData"></core:Fragment>
								</objPage:blocks>
							</objPage:ObjectPageSubSection>
						</objPage:subSections>
					</objPage:ObjectPageSection>
					<objPage:ObjectPageSection title="{i18n>SalesData}" id="ID_OP_SALES_DATA">
						<objPage:subSections>
							<objPage:ObjectPageSubSection>
								<objPage:blocks>
									<core:Fragment type="XML" fragmentName="MDMZCUSTOMER.fragments.salesData"></core:Fragment>
								</objPage:blocks>
							</objPage:ObjectPageSubSection>
						</objPage:subSections>
					</objPage:ObjectPageSection>
					<objPage:ObjectPageSection title="{i18n>StatusData}" id="ID_OP_STUS_DATA">
						<objPage:subSections>
							<objPage:ObjectPageSubSection>
								<objPage:blocks>
									<core:Fragment type="XML" fragmentName="MDMZCUSTOMER.fragments.statusData"></core:Fragment>
								</objPage:blocks>
							</objPage:ObjectPageSubSection>
						</objPage:subSections>
					</objPage:ObjectPageSection>
					<objPage:ObjectPageSection title="{i18n>TextNotes}" id="ID_OP_TEXT_NOTE">
						<objPage:subSections>
							<objPage:ObjectPageSubSection>
								<objPage:blocks>
									<core:Fragment type="XML" fragmentName="MDMZCUSTOMER.fragments.textNotes"></core:Fragment>
								</objPage:blocks>
							</objPage:ObjectPageSubSection>
						</objPage:subSections>
					</objPage:ObjectPageSection>
					<objPage:ObjectPageSection title="{i18n>UploadDocuments}" id="ID_OP_UPLOD_DOC">
						<objPage:subSections>
							<objPage:ObjectPageSubSection>
								<objPage:blocks>
									<core:Fragment type="XML" fragmentName="MDMZCUSTOMER.fragments.documentUpload"></core:Fragment>
								</objPage:blocks>
							</objPage:ObjectPageSubSection>
						</objPage:subSections>
					</objPage:ObjectPageSection>
				</objPage:sections>
			</objPage:ObjectPageLayout>
		</content>
		<footer>
			<Toolbar>
				<ToolbarSpacer></ToolbarSpacer>
				<Button text="{i18n>changesubPage}" tooltip="{i18n>changesubPage}" icon="sap-icon://edit" type="Emphasized" press="onClickEditRecod"
					visible="{ButtonPropertyModel>/changeVisible}"/>
				<!--visible="{parts:[{path:'TabSelectionModel>/SelecteIconTab'},{path:'RequiredOptionalSet>/UploadDoc'}],formatter:'MDMZCUSTOMER.model.Formatter.ChengeBtnVisble'}"/>-->
				<Button text="{i18n>DuplicateCheckBtnText}" tooltip="{i18n>DuplicateCheckBtnText}" enabled="{DuplicateCheckSet>/DuplicateBtnEnable}"
					type="Accept" icon="sap-icon://survey" press="duplicateCheckButonPress" visible="{ButtonPropertyModel>/duplicateCheckVisible}"/>
				<!--visible="{parts:[{path:'TabSelectionModel>/SelecteIconTab'},{path:'RequiredOptionalSet>/UploadDoc'}],formatter:'MDMZCUSTOMER.model.Formatter.duplicateCheckBtnVisble'}"/>-->
				<Button text="{i18n>sendForReview}" tooltip="{i18n>sendForReview}" type="Emphasized" icon="sap-icon://survey" press="onPressSendForReview"
					visible="{ButtonPropertyModel>/sendForReviewVisible}"/>
				<!--visible="{parts:[{path:'TabSelectionModel>/SelecteIconTab'},{path:'RequiredOptionalSet>/UploadDoc'}],formatter:'MDMZCUSTOMER.model.Formatter.sendForReviewVisble'}"/>-->
				<Button text="{i18n>approveBtn}" tooltip="{i18n>approveBtn}" type="Emphasized" icon="sap-icon://accept" press="onApprove"
					enabled="{ButtonPropertyModel>/Enabled}" visible="{ButtonPropertyModel>/approveVisible}"/>
				<Button text="{i18n>rejectBtn}" tooltip="{i18n>rejectBtn}" type="Reject" press="onReject" icon="sap-icon://cancel"
					enabled="{ButtonPropertyModel>/Enabled}" visible="{ButtonPropertyModel>/rejectVisible}"/>
				<Button text="{i18n>cancelBtn}" tooltip="{i18n>cancelBtn}" type="Reject" press="onDelete" icon="sap-icon://cancel"
					enabled="{ButtonPropertyModel>/DeleteEnabled}" visible="{ButtonPropertyModel>/DeleteVisible}"/>
				<Button text="{i18n>closeBtn}" tooltip="{i18n>closeBtn}" type="Reject" press="onClose" icon="sap-icon://decline"
					enabled="{ButtonPropertyModel>/closeEnabled}" visible="{ButtonPropertyModel>/closeVisible}"/>
				<Button text="{i18n>cancelBtn}" tooltip="{i18n>cancelBtn}" type="Reject" press="onCancelRequest" icon="sap-icon://decline"
					visible="{ButtonPropertyModel>/cancelVisible}"/>
			</Toolbar>
		</footer>
	</Page>
	<!--</pages>
	</App>-->
</mvc:View>