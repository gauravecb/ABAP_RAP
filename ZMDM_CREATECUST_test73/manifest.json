{
    "_version": "1.7.0",
    "sap.app": {
        "id": "MDMZCUSTOMER",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "servicecatalog.connectivityComponent",
            "version": "0.0.0"
        },
        "dataSources": {
            "ZMDM_BUSINESSPARTNER_SRV": {
                "uri": "/sap/opu/odata/sap/ZMDM_BUSINESSPARTNER_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "MDMZCUSTOMER.view.App",
            "type": "XML",
            "async": true,
            "id": "idAppControl",
            "height": "100%"
        },
        "dependencies": {
            "minUI5Version": "1.30.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {},
                "sap.ushell": {},
                "sap.collaboration": {},
                "sap.ui.comp": {},
                "sap.uxap": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "MDMZCUSTOMER.i18n.i18n"
                }
            },
            "BusinessPartner": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "OneTime",
                    "defaultCountMode": "Request",
                    "useBatch": false
                },
                "dataSource": "ZMDM_BUSINESSPARTNER_SRV",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "MDMZCUSTOMER.view",
                "controlAggregation": "pages",
                "controlId": "idAppControl",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "searchCustomer",
                    "pattern": "",
                    "target": [
                        "searchCustomer"
                    ]
                },
                {
                    "name": "createCustomer",
                    "pattern": "createCustomer",
                    "target": [
                        "createCustomer"
                    ]
                },
                {
                    "name": "ExBussSubscription",
                    "pattern": "ExBussSubscription({contextPath})",
                    "target": [
                        "ExBussSubscription"
                    ]
                },
                {
                    "name": "Display",
                    "pattern": "Display",
                    "target": [
                        "Display"
                    ]
                },
                {
                    "name": "changeCustomer",
                    "pattern": "changeCustomer",
                    "target": [
                        "changeCustomer"
                    ]
                },
                {
                    "name": "NewSubscription",
                    "pattern": "NewSubscription",
                    "target": [
                        "NewSubscription"
                    ]
                },
                {
                    "name": "Extension",
                    "pattern": "Extension",
                    "target": [
                        "Extension"
                    ]
                }
            ],
            "targets": {
                "searchCustomer": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "searchCustomer"
                },
                "createCustomer": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "createCustomer"
                },
                "App": {
                    "viewType": "XML",
                    "viewName": "App"
                },
                "ExBussSubscription": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "ExBussSubscription"
                },
                "Display": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "Display"
                },
                "changeCustomer": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "changeCustomer"
                },
                "NewSubscription": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "NewSubscription"
                },
                "Extension": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "Extension"
                }
            }
        }
    },
    "sap.platform.abap": {
        "uri": "/sap/bc/ui5_ui5/sap/zmdm_createcust",
        "_version": "1.1.0"
    }
}