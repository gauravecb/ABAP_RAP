<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="{PanelModelSet>/SalesOrderPanel}">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>SalesOrder}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<f:SimpleForm class="editableForm" editable="true" layout="ResponsiveGridLayout" labelSpanL="4" labelSpanM="8" labelSpanS="6"
			adjustLabelSpan="false" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
			<f:content>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>SalesDistrict}"
					required="{path:'RequiredOptionalSet>/SalesDistrictSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox items="{SalesDistrictSet>/results}" placeholder="{i18n>select} {i18n>SalesDistrict}"
					change="onLiveChangeofSalesDistrict" selectedKey="{SalesDataSet>/SalesOrder/SalesDistrict}"
					valueState="{SalesDataSet>/SalesOrder/SalesDistrictState}" valueStateText="{SalesDataSet>/SalesOrder/SalesDistrictMessage}"
					enabled="{path:'RequiredOptionalSet>/SalesDistrictSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/SalesOrder/SalesDistrictTooltip}">
					<core:Item key="{SalesDistrictSet>Sales_District}" text="{SalesDistrictSet>Sales_District}-{SalesDistrictSet>District_Name}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>SalesOffice}"
					required="{path:'RequiredOptionalSet>/SalesOfficeSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox items="{SalesOfficeSet>/results}" placeholder="{i18n>select} {i18n>SalesOffice}"
					change="onLiveChangeofSalesOffice" selectedKey="{SalesDataSet>/SalesOrder/SalesOffice}"
					valueState="{SalesDataSet>/SalesOrder/SalesOfficeState}" valueStateText="{SalesDataSet>/SalesOrder/SalesOfficeMessage}"
					enabled="{path:'RequiredOptionalSet>/SalesOfficeSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/SalesOrder/SalesOfficeTooltip}">
					<core:Item key="{SalesOfficeSet>Sales_Office}" text="{SalesOfficeSet>Sales_Office}-{SalesOfficeSet>Sales_Office_Desc}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>SalesGroup}"
					required="{path:'RequiredOptionalSet>/SalesGroupSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox items="{SalesGroupSet>/results}" placeholder="{i18n>select} {i18n>SalesGroup}"
					change="onLiveChangeofSalesGroup" selectedKey="{SalesDataSet>/SalesOrder/SalesGroup}"
					valueState="{SalesDataSet>/SalesOrder/SalesGroupState}" valueStateText="{SalesDataSet>/SalesOrder/SalesGroupMessage}"
					enabled="{path:'RequiredOptionalSet>/SalesGroupSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/SalesOrder/SalesGroupTooltip}">
					<core:Item key="{SalesGroupSet>Vkgrp}" text="{SalesGroupSet>Vkgrp}-{SalesGroupSet>Bezei}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>CustomerGroup}"
					required="{path:'RequiredOptionalSet>/CustomerGroupSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox items="{CustomerGroupSet>/results}" placeholder="{i18n>select} {i18n>CustomerGroup}"
					change="onLiveChangeofCustomerGroup" selectedKey="{SalesDataSet>/SalesOrder/CustomerGroup}"
					valueState="{SalesDataSet>/SalesOrder/CustomerGroupState}" valueStateText="{SalesDataSet>/SalesOrder/CustomerGroupMessage}"
					enabled="{path:'RequiredOptionalSet>/CustomerGroupSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/SalesOrder/CustomerGroupTooltip}">
					<core:Item key="{CustomerGroupSet>Kdgrp}" text="{CustomerGroupSet>Kdgrp}-{CustomerGroupSet>Ktext}"/>
				</ComboBox>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>Currency}"
					required="{path:'RequiredOptionalSet>/CurrencySLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox items="{CurrencySet>/results}" placeholder="{i18n>select} {i18n>Currency}"
					change="onLiveChangeofCurrency" selectedKey="{SalesDataSet>/SalesOrder/Currency}" valueState="{SalesDataSet>/SalesOrder/CurrencyState}"
					valueStateText="{SalesDataSet>/SalesOrder/CurrencyMessage}"
					enabled="{path:'RequiredOptionalSet>/CurrencySLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/SalesOrder/CurrencyTooltip}">
					<core:Item key="{CurrencySet>Waers}" text="{CurrencySet>Waers}-{CurrencySet>Ltext}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>AcctAtCust}"
					required="{path:'RequiredOptionalSet>/AcctAtCustSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<Input maxLength="12" value = "{SalesDataSet>/SalesOrder/AcctAtCust}" liveChange="onLiveChangeofAcctAtCustState" valueState="{SalesDataSet>/SalesOrder/AcctAtCustState}"
					valueStateText="{SalesDataSet>/SalesOrder/AcctAtCustMessage}"
					enabled="{path:'RequiredOptionalSet>/AcctAtCustSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/SalesOrder/AcctAtCustTooltip}"/>
				<Label textAlign="Left" text="{i18n>ExchangeRateType}"
					required="{path:'RequiredOptionalSet>/ExchangeRateTypeSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox items="{ExchangeRateTypeSet>/results}" placeholder="{i18n>select} {i18n>ExchangeRateType}"
					change="onLiveChangeofExchangeRateType" selectedKey="{SalesDataSet>/SalesOrder/ExchangeRateType}"
					valueState="{SalesDataSet>/SalesOrder/ExchangeRateTypeState}" valueStateText="{SalesDataSet>/SalesOrder/ExchangeRateTypeMessage}"
					enabled="{path:'RequiredOptionalSet>/ExchangeRateTypeSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/SalesOrder/ExchangeRateTypeTooltip}">
					<core:Item key="{ExchangeRateTypeSet>Exchratetype}" text="{ExchangeRateTypeSet>Exchratetype}-{ExchangeRateTypeSet>Usage}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>PriceGroup}"
					required="{path:'RequiredOptionalSet>/PriceGroupSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox items="{PriceGroupSet>/results}" placeholder="{i18n>select} {i18n>PriceGroup}"
					change="onLiveChangeofPriceGroup" selectedKey="{SalesDataSet>/SalesOrder/PriceGroup}"
					valueState="{SalesDataSet>/SalesOrder/PriceGroupState}" valueStateText="{SalesDataSet>/SalesOrder/PriceGroupMessage}"
					enabled="{path:'RequiredOptionalSet>/PriceGroupSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/SalesOrder/PriceGroupTooltip}">
					<core:Item key="{PriceGroupSet>Pricegroup}" text="{PriceGroupSet>Pricegroup}-{PriceGroupSet>Description}"/>
				</ComboBox>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>CustPricProc}"
					required="{path:'RequiredOptionalSet>/CustPricProcSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox items="{CustPricProcSet>/results}" placeholder="{i18n>select} {i18n>CustPricProc}"
					change="onLiveChangeofCustPricProc" selectedKey="{SalesDataSet>/SalesOrder/CustPricProc}"
					valueState="{SalesDataSet>/SalesOrder/CustPricProcState}" valueStateText="{SalesDataSet>/SalesOrder/CustPricProcMessage}"
					enabled="{path:'RequiredOptionalSet>/CustPricProcSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/SalesOrder/CustPricProcTooltip}">
					<core:Item key="{CustPricProcSet>Kalks}" text="{CustPricProcSet>Kalks}-{CustPricProcSet>Vtext}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>PriceList}"
					required="{path:'RequiredOptionalSet>/PriceListSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox items="{PriceListSet>/results}" placeholder="{i18n>select} {i18n>PriceList}"
					change="onLiveChangeofPriceList" selectedKey="{SalesDataSet>/SalesOrder/PriceList}" valueState="{SalesDataSet>/SalesOrder/PriceListState}"
					valueStateText="{SalesDataSet>/SalesOrder/PriceListMessage}"
					enabled="{path:'RequiredOptionalSet>/PriceListSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/SalesOrder/PriceListTooltip}">
					<core:Item key="{PriceListSet>Pricelist}" text="{PriceListSet>Pricelist}-{PriceListSet>Description}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>CustStatGrp}"
					required="{path:'RequiredOptionalSet>/CustStatGrpSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox items="{CustStatGrpSet>/results}" placeholder="{i18n>select} {i18n>CustStatGrp}"
					change="onLiveChangeofCustStatGrp" selectedKey="{SalesDataSet>/SalesOrder/CustStatGrp}"
					valueState="{SalesDataSet>/SalesOrder/CustStatGrpState}" valueStateText="{SalesDataSet>/SalesOrder/CustStatGrpMessag}"
					enabled="{path:'RequiredOptionalSet>/CustStatGrpSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/SalesOrder/CustStatGrpTooltip}">
					<core:Item key="{CustStatGrpSet>Stgku}" text="{CustStatGrpSet>Stgku}-{CustStatGrpSet>Bezei20}"/>
				</ComboBox>
			</f:content>
		</f:SimpleForm>
	</Panel>
</core:FragmentDefinition>