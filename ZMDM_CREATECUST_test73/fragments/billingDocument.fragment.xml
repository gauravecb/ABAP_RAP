<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="{PanelModelSet>/BillingDocumentPanel}">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>BillingDocument}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<f:SimpleForm class="customClassTitle editableForm" editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8"
			labelSpanS="6" adjustLabelSpan="false" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
			<f:content>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>Rebate}"
					required="{path:'RequiredOptionalSet>/RebateSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}" wrapping="true"/>
				<CheckBox width="10%"
				select="onselectRebat"
				selected="{SalesDataSet>/BillingDocument/Rebate}"
				valueState="{SalesDataSet>/BillingDocument/RebateState}" valueStateText="{SalesDataSet>/BillingDocument/RebateMessage}"
					enabled="{path:'RequiredOptionalSet>/RebateSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"/>
				<Label textAlign="Left" text="{i18n>PriceDetermination}" wrapping="true"
					required="{path:'RequiredOptionalSet>/PriCeDeterminCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<CheckBox width="10%" valueState="{SalesDataSet>/BillingDocument/PriceDeterminationState}"
					select="onselectPriceDetrmination"
					selected="{SalesDataSet>/BillingDocument/PriceDetermination}"
					enabled="{path:'RequiredOptionalSet>/PriCeDeterminCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					valueStateText="{SalesDataSet>/BillingDocument/PriceDeterminationMessage}"/>
				<Label textAlign="Left" text="{i18n>InvoiceDates}"
					required="{path:'RequiredOptionalSet>/InvoiceDatesSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}" wrapping="true"/>
				<ComboBox items="{InvoiceDatesSDBillDocSet>/results}" placeholder="{i18n>select} {i18n>InvoiceDates}"
					change = "onLiveChangeOfInvoiceDates"
					selectedKey="{SalesDataSet>/BillingDocument/InvoiceDates}" valueState="{SalesDataSet>/BillingDocument/InvoiceDatesState}"
					valueStateText="{SalesDataSet>/BillingDocument/InvoiceDatesMessage}"
					enabled="{path:'RequiredOptionalSet>/InvoiceDatesSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/BillingDocument/InvoiceDatesTooltip}">
					<core:Item key="{InvoiceDatesSDBillDocSet>Ident}" text="{InvoiceDatesSDBillDocSet>Ident}-{InvoiceDatesSDBillDocSet>Ltext}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>InvoiceListDates}"
					required="{path:'RequiredOptionalSet>/InvoiceListDatesSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}" wrapping="true"/>
				<ComboBox items="{InvoiceListDatesSDBillDocSet>/results}" placeholder="{i18n>select} {i18n>InvoiceListDates}"
				    change = "onLiveChangeOfInvoiceListDates"
					selectedKey="{SalesDataSet>/BillingDocument/InvoiceListDates}" valueState="{SalesDataSet>/BillingDocument/InvoiceListDatesState}"
					valueStateText="{SalesDataSet>/BillingDocument/InvoiceListDatesMessage}"
					enabled="{path:'RequiredOptionalSet>/InvoiceListDatesSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/BillingDocument/InvoiceListDatesTooltip}">
					<core:Item key="{InvoiceListDatesSDBillDocSet>Ident}" text="{InvoiceListDatesSDBillDocSet>Ident}-{InvoiceListDatesSDBillDocSet>Ltext}"/>
				</ComboBox>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>Incoterms1}"
					required="{path:'RequiredOptionalSet>/Incoterms1SLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}" wrapping="true"/>
				<ComboBox items="{IncotermsSDBillDocSet>/results}" placeholder="{i18n>select} {i18n>Incoterms1}"
					 change = "onLiveChangeOfIncoterms1"
					selectedKey="{SalesDataSet>/BillingDocument/Incoterms1}" valueState="{SalesDataSet>/BillingDocument/Incoterms1State}"
					valueStateText="{SalesDataSet>/BillingDocument/Incoterms1Message}"
					enabled="{path:'RequiredOptionalSet>/Incoterms1SLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/BillingDocument/Incoterms1Tooltip}">
					<core:Item key="{IncotermsSDBillDocSet>Inco1}" text="{IncotermsSDBillDocSet>Inco1}-{IncotermsSDBillDocSet>Bezei}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>Incoterms2}"
					required="{path:'RequiredOptionalSet>/Incoterms2SLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}" wrapping="true"/>
				<Input
				maxLength="28"
				valueState="{SalesDataSet>/BillingDocument/Incoterms2State}" valueStateText="{SalesDataSet>/BillingDocument/Incoterms2Message}"
				  liveChange = "onLiveChangeOfIncoterms2" value="{SalesDataSet>/BillingDocument/Incoterms2}"
					enabled="{path:'RequiredOptionalSet>/Incoterms2SLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/BillingDocument/Incoterms2Tooltip}"/>
				<Label textAlign="Left" text="{i18n>TermsofPayment}"
					required="{path:'RequiredOptionalSet>/TermsofPaymentSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}" wrapping="true"/>
				<ComboBox items="{TermsofPaymentSDBillDocSet>/results}" placeholder="{i18n>select} {i18n>TermsofPayment}"
					change = "onLiveChangeOfTermsofPayment"
					selectedKey="{SalesDataSet>/BillingDocument/TermsofPayment}" valueState="{SalesDataSet>/BillingDocument/TermsofPaymentState}"
					valueStateText="{SalesDataSet>/BillingDocument/TermsofPaymentMessage}"
					enabled="{path:'RequiredOptionalSet>/TermsofPaymentSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/BillingDocument/TermsofPaymentTooltip}">
					<core:Item key="{TermsofPaymentSDBillDocSet>PaymentTerms}" text="{TermsofPaymentSDBillDocSet>PaymentTerms}-{TermsofPaymentSDBillDocSet>Text}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>AccountAssignmentGroup}"
					required="{path:'RequiredOptionalSet>/AccountAssignmentGroupSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"
					wrapping="true"/>
				<ComboBox items="{AccountAssignmentGroupSDBillDocSet>/results}"
					change = "onLiveChangeOfAccountAssignmentGroup"
					placeholder="{i18n>select} {i18n>AccountAssignmentGroup}" selectedKey="{SalesDataSet>/BillingDocument/AccountAssignmentGroup}"
					valueState="{SalesDataSet>/BillingDocument/AccountAssignmentGroupState}"
					valueStateText="{SalesDataSet>/BillingDocument/AccountAssignmentGroupMessage}"
					enabled="{path:'RequiredOptionalSet>/AccountAssignmentGroupSLDCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					tooltip="{SalesDataSet>/BillingDocument/AccountAssignmentGroupTooltip}">
					<core:Item key="{AccountAssignmentGroupSDBillDocSet>Ktgrd}" text="{AccountAssignmentGroupSDBillDocSet>Ktgrd}-{AccountAssignmentGroupSDBillDocSet>Vtext}"/>
				</ComboBox>
				<core:Title/>
				<Link press="displayTaxClassificationDialog" text="{i18n>TaxCountryClassification}"></Link>
				<!--<Label textAlign="Left" text="{i18n>TaxCountry}" wrapping="true"/>-->
				<!--<ComboBox items="">-->
				<!--	<core:Item key="" text=""/>-->
				<!--</ComboBox>-->
				<!--<Label textAlign="Left" text="{i18n>TaxClassification}" wrapping="true"/>-->
				<!--<Input />-->
			</f:content>
		</f:SimpleForm>
	</Panel>
</core:FragmentDefinition>