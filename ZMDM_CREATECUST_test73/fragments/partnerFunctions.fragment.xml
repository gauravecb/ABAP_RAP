<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="{PanelModelSet>/PartnerFunctionsPanel}">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>PartnerFunctions}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<Label text="{i18n>DefaultPartnerfunction}"/>
		<f:SimpleForm class="customClassTitle editableForm" editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8"
			labelSpanS="6" adjustLabelSpan="false" emptySpanL="0" emptySpanM="0" columnsL="1" columnsM="1" columnsS="1">
			<f:content>
				<core:Title/>
				<Table enableBusyIndicator="false" items="{DefaultGeneratedPartnerFnSet>/results/d/results}">
					<columns>
						<Column demandPopin="true" minScreenWidth="Tablet">
							<Label text="{i18n>PartnerFunction}"
								required="{path:'RequiredOptionalSet>/PartFunDGCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
						<Column demandPopin="true" minScreenWidth="Tablet">
							<Label text="{i18n>Number}" required="{path:'RequiredOptionalSet>/NumDGCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
						<Column demandPopin="true" minScreenWidth="Tablet">
							<Label text="{i18n>Name}" required="{path:'RequiredOptionalSet>/NameDGCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
						<Column demandPopin="true" minScreenWidth="Tablet">
							<Label text="{i18n>PartnerDescription}"
								required="{path:'RequiredOptionalSet>/PartDescDGCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
						<Column demandPopin="true" minScreenWidth="Tablet">
							<Label text="{i18n>Default}" required="{path:'RequiredOptionalSet>/DefaultDGCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
					</columns>
					<items>
						<ColumnListItem>
							<Text text="{DefaultGeneratedPartnerFnSet>PartnerName}" valueStateText="{DefaultGeneratedPartnerFnSet>PartFunMessage}"
								valueState="{DefaultGeneratedPartnerFnSet>PartFunState}"
								enabled="{path:'RequiredOptionalSet>/PartFunDGCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
								tooltip="{DefaultGeneratedPartnerFnSet>PartFunTooltip}"/>
							<Input
							maxLength="10"
							value="{DefaultGeneratedPartnerFnSet>Number}" change="onChangeCustNumDefaultPartFn"
								editable="{DefaultGeneratedPartnerFnSet>Editable}" valueStateText="{DefaultGeneratedPartnerFnSet>NumMessage}"
								valueState="{DefaultGeneratedPartnerFnSet>NumState}"
								enabled="{path:'RequiredOptionalSet>/NumDGCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
								tooltip="{DefaultGeneratedPartnerFnSet>NumTooltip}"/>
							<Input value="{DefaultGeneratedPartnerFnSet>Name}" valueStateText="{DefaultGeneratedPartnerFnSet>NameMessage}"
								valueState="{DefaultGeneratedPartnerFnSet>NameState}" liveChange="liveChangeNameDefaultGeneratedPartne" editable="false"
								enabled="{path:'RequiredOptionalSet>/NameDGCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
								tooltip="{DefaultGeneratedPartnerFnSet>NameTooltip}"/>
							<Input 
							maxLength="30"
							value="{DefaultGeneratedPartnerFnSet>PartnerDescription}" valueStateText="{DefaultGeneratedPartnerFnSet>PartDescMessage}"
								valueState="{DefaultGeneratedPartnerFnSet>PartDescState}" liveChange="liveChangePartnerDescDefaultGeneratedPartne"
								enabled="{path:'RequiredOptionalSet>/PartDescDGCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
								tooltip="{DefaultGeneratedPartnerFnSet>PartDescTooltip}"/>
							<!--<RadioButton groupName="GroupDefaultPartnerFunction" selected="{DefaultGeneratedPartnerFnSet>Default}"-->
							<!--select="liveChangeDefaultSelectDefaultGeneratedPartne"-->
							<!--	valueStateText="{DefaultGeneratedPartnerFnSet>DefaultMessage}" valueState="{DefaultGeneratedPartnerFnSet>DefaultState}"-->
							<!--	enabled="{path:'RequiredOptionalSet>/DefaultDGCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"/>-->
							<CheckBox selected="{DefaultGeneratedPartnerFnSet>Default}" select="liveChangeDefaultSelectDefaultGeneratedPartne"
								valueStateText="{DefaultGeneratedPartnerFnSet>DefaultMessage}" valueState="{DefaultGeneratedPartnerFnSet>DefaultState}"
								enabled="{path:'RequiredOptionalSet>/DefaultDGCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"/>
						</ColumnListItem>
					</items>
				</Table>
			</f:content>
		</f:SimpleForm>
		<Toolbar>
			<Label text="{i18n>OtherPartnerFunction}" visible="{OtherPartnerFunctionSet>/DisplayOtherPartnerFunctions}"></Label>
			<ToolbarSpacer></ToolbarSpacer>
			<Button text="+"
				visible="{parts:[{path:'OtherPartnerFunctionSet>/DisplayOtherPartnerFunctions'},{path:'RequiredOptionalSet>/PartFunEdit'}],formatter:'MDMZCUSTOMER.model.Formatter.partnerFunBtnEnable'}"
				press="addOtherPartnerFunctions" width="5%" type="Accept"/>
		</Toolbar>
		<f:SimpleForm class="customClassTitle editableForm" editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8"
			labelSpanS="6" adjustLabelSpan="false" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3"
			visible="{OtherPartnerFunctionSet>/DisplayOtherPartnerFunctions}">
			<f:content>
				<Table enableBusyIndicator="false" items="{OtherPartnerFunctionSet>/OtherPartnerFunctions}">
					<columns>
						<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
							<Label text="{i18n>PartnerFunction}"
								required="{path:'RequiredOptionalSet>/PartFunOPCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
						<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
							<Label text="{i18n>Number}" required="{path:'RequiredOptionalSet>/NumOPCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
						<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
							<Label text="{i18n>Name}" required="{path:'RequiredOptionalSet>/NameOPCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
						<Column demandPopin="true" minScreenWidth="Tablet" width="40%">
							<Label text="{i18n>PartnerDescription}"
								required="{path:'RequiredOptionalSet>/PartDescOPCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
						<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
							<Label text="{i18n>Default}" required="{path:'RequiredOptionalSet>/DefaultOPCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
					</columns>
					<items>
						<ColumnListItem>
							<ComboBox items="{PartnerFunctionComboSet>/results/d/results/0/NavOtherPartnerFunction/results}"
								placeholder="{i18n>select} {i18n>PartnerFunction}" selectedKey="{OtherPartnerFunctionSet>PartnerFunction}"
								change="onChangeCustNumOtherPartFn" valueStateText="{OtherPartnerFunctionSet>PartFunMessage}"
								valueState="{OtherPartnerFunctionSet>PartFunState}"
								enabled="{path:'RequiredOptionalSet>/PartFunDGCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
								tooltip="{OtherPartnerFunctionSet>PartFunTooltip}">
								<core:Item key="{PartnerFunctionComboSet>PartnerFunction}"
									text="{PartnerFunctionComboSet>PartnerFunction}-{PartnerFunctionComboSet>PartnerDescription}"/>
							</ComboBox>
							<Input 
							maxLength="10"
							valueStateText="{OtherPartnerFunctionSet>NumMessage}" valueState="{OtherPartnerFunctionSet>NumState}"
								value="{OtherPartnerFunctionSet>Number}" change="onChangeCustNumOtherPartFn"
								enabled="{path:'RequiredOptionalSet>/NumOPCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
								tooltip="{OtherPartnerFunctionSet>NumTooltip}"/>
							<Input valueStateText="{OtherPartnerFunctionSet>NameMessage}" valueState="{OtherPartnerFunctionSet>NameState}"
								value="{OtherPartnerFunctionSet>Name}" editable="false" liveChange="liveChangeNameOPF"
								enabled="{path:'RequiredOptionalSet>/NameOPCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
								tooltip="{OtherPartnerFunctionSet>NameTooltip}"/>
							<Input 
							maxLength="30"
							valueStateText="{OtherPartnerFunctionSet>PartDescMessage}" valueState="{OtherPartnerFunctionSet>PartDescState}"
								value="{OtherPartnerFunctionSet>PartnerDescriptionText}" liveChange="liveChangePartDescOPF"
								enabled="{path:'RequiredOptionalSet>/PartDescOPCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
								tooltip="{OtherPartnerFunctionSet>PartDescTooltip}"/>
							<!--<RadioButton valueStateText="{OtherPartnerFunctionSet>DefaultMessage}" valueState="{OtherPartnerFunctionSet>DefaultState}"-->
							<!--	groupName="OtherPartnerFunction" selected="{OtherPartnerFunctionSet>Default}"-->
							<!--	select="selectDefaultOPF"-->
							<!--	enabled="{path:'RequiredOptionalSet>/DefaultOPCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"/>-->
							<Checkbox valueStateText="{OtherPartnerFunctionSet>DefaultMessage}" valueState="{OtherPartnerFunctionSet>DefaultState}"
								selected="{OtherPartnerFunctionSet>Default}" select="selectDefaultOPF"
								enabled="{path:'RequiredOptionalSet>/DefaultOPCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"/>
						</ColumnListItem>
					</items>
				</Table>
			</f:content>
		</f:SimpleForm>
		<Toolbar>
			<Label text="{i18n>LinkedToSoldToParty}" visible="{SoldtoPartnerFunctionSet>/DisplaySoldtoPartnerFunction}"></Label>
			<ToolbarSpacer></ToolbarSpacer>
			<Button text="+"
				visible="{parts:[{path:'SoldtoPartnerFunctionSet>/DisplaySoldtoPartnerFunction'},{path:'RequiredOptionalSet>/PartFunEdit'}],formatter:'MDMZCUSTOMER.model.Formatter.partnerFunBtnEnable'}"
				press="addSoldtoPartnerFunction" width="5%" type="Accept"/>
		</Toolbar>
		<f:SimpleForm class="customClassTitle editableForm" editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8"
			labelSpanS="6" adjustLabelSpan="false" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3"
			visible="{SoldtoPartnerFunctionSet>/DisplaySoldtoPartnerFunction}">
			<f:content>
				<Table enableBusyIndicator="false" items="{SoldtoPartnerFunctionSet>/SoldtoPartnerFunctions}">
					<columns>
						<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
							<Label text="{i18n>Number}" required="{path:'RequiredOptionalSet>/NumSOPCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
						<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
							<Label text="{i18n>Name}" required="{path:'RequiredOptionalSet>/NameSOPCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
						<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
							<Label text="{i18n>PartnerFunction}"
								required="{path:'RequiredOptionalSet>/PartFunSOPCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
						<Column demandPopin="true" minScreenWidth="Tablet" width="40%">
							<Label text="{i18n>PartnerDescription}"
								required="{path:'RequiredOptionalSet>/PartDescSOPCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
						<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
							<Label text="{i18n>Default}" required="{path:'RequiredOptionalSet>/DefaultSOPCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
						</Column>
					</columns>
					<items>
						<ColumnListItem>
							<Input 
							maxLength="10"
							value="{SoldtoPartnerFunctionSet>Number}" valueStateText="{SoldtoPartnerFunctionSet>NumMessage}"
								valueState="{SoldtoPartnerFunctionSet>NumState}"
								enabled="{path:'RequiredOptionalSet>/NumSOPCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
								change="onChangeCustNumSoldToPartFn"
								tooltip="{SoldtoPartnerFunctionSet>NumTooltip}"/>
							<Input valueStateText="{SoldtoPartnerFunctionSet>NameMessage}" valueState="{SoldtoPartnerFunctionSet>NameState}"
								value="{SoldtoPartnerFunctionSet>Name}" editable="false" liveChange="liveChangeNameSOP"
								enabled="{path:'RequiredOptionalSet>/NameSOPCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
								tooltip="{SoldtoPartnerFunctionSet>NameTooltip}"/>
							<ComboBox items="{PartnerFunctionComboSet>/results/d/results/0/NavOtherPartnerFunction/results}"
								placeholder="{i18n>select} {i18n>PartnerFunction}" selectedKey="{SoldtoPartnerFunctionSet>PartnerFunction}"
								change="onChangeCustNumSoldToPartFn" valueStateText="{SoldtoPartnerFunctionSet>PartFunMessage}"
								valueState="{SoldtoPartnerFunctionSet>PartFunState}"
								enabled="{path:'RequiredOptionalSet>/PartFunSOPCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
								tooltip="{SoldtoPartnerFunctionSet>PartFunTooltip}">
								<core:Item key="{PartnerFunctionComboSet>PartnerFunction}"
									text="{PartnerFunctionComboSet>PartnerFunction}-{PartnerFunctionComboSet>PartnerDescription}"/>
							</ComboBox>
							<Input
							maxLength="30"
							liveChange="liveChangePartDescSOP" valueStateText="{SoldtoPartnerFunctionSet>PartDescMessage}"
								valueState="{SoldtoPartnerFunctionSet>PartDescState}" value="{SoldtoPartnerFunctionSet>PartnerDescriptionText}"
								enabled="{path:'RequiredOptionalSet>/PartDescSOPCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
								tooltip="{SoldtoPartnerFunctionSet>PartDescTooltip}"/>
							<!--<RadioButton select="onSelectDefaultSOP" valueStateText="{SoldtoPartnerFunctionSet>DefaultMessage}" valueState="{SoldtoPartnerFunctionSet>DefaultState}"-->
							<!--	groupName="SoldToPartnerFunction" selected="{SoldtoPartnerFunctionSet>Default}"-->
							<!--	enabled="{path:'RequiredOptionalSet>/DefaultSOPCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"/>-->
							<CheckBox select="onSelectDefaultSOP" valueStateText="{SoldtoPartnerFunctionSet>DefaultMessage}"
								valueState="{SoldtoPartnerFunctionSet>DefaultState}" selected="{SoldtoPartnerFunctionSet>Default}"
								enabled="{path:'RequiredOptionalSet>/DefaultSOPCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"/>
						</ColumnListItem>
					</items>
				</Table>
			</f:content>
		</f:SimpleForm>
	</Panel>
</core:FragmentDefinition>