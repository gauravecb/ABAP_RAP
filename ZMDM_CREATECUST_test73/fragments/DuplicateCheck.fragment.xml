<core:FragmentDefinition xmlns="sap.m" xmlns:u="sap.ui.table" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<Dialog title="{i18n>duplicateCheckHeading}" contentWidth="100%" class="sapUiSizeCompact">
		<content>
			<TextArea value="{DuplicateCommentModelSet>/Comment}" enabled="false" width="100%"
				visible="{ButtonPropertyModel>/duplicateCheckCommentVisible}"></TextArea>
			<Table enableBusyIndicator="false" select="onSelctChangedupCust" items="{DuplCheckTableDataSet>/results}" mode="SingleSelectLeft">
				<columns>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>goldenRecIdDupCheck}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet" width="15%">
						<Text text="{i18n>nameDupCheck}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>streetdupCheck}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>houseNumDupCheck}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>postalCodeDupCheck}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>cityDupChek}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>distreectDupCheck}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>countryDupCheck}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>poBoxDupCheck}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>POBoxPostalCodeDupCheck}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>poBoxCityDupChek}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<Text text="{DuplCheckTableDataSet>BSNS_PRTNR_ID}"></Text>
						<Text text="{DuplCheckTableDataSet>NAME}"></Text>
						<Text text="{DuplCheckTableDataSet>STREET}"></Text>
						<Text text="{DuplCheckTableDataSet>HOUSE_NUMBER}"></Text>
						<Text text="{DuplCheckTableDataSet>POSTAL_CODE}"></Text>
						<Text text="{DuplCheckTableDataSet>CITY}"></Text>
						<Text text="{DuplCheckTableDataSet>DISTRICT}"></Text>
						<Text text="{DuplCheckTableDataSet>COUNTRY}"></Text>
						<Text text="{DuplCheckTableDataSet>BOX_NUMBER}"></Text>
						<Text text="{DuplCheckTableDataSet>PO_BOX_POSTAL_CODE}"></Text>
						<Text text="{DuplCheckTableDataSet>PO_BOX_CITY}"></Text>
					</ColumnListItem>
				</items>
			</Table>
		</content>
		<Toolbar>
			<ToolbarSpacer></ToolbarSpacer>
			<Button text="{i18n>Change}" press="HandleChangeCustomerDuplicate" tooltip="{i18n>Change}"
				visible="{ButtonPropertyModel>/duplicateCheckChangeBtnVisible}"/>
			<Button text="{i18n>precessNew}" press="HandleNewustomerDuplicate" tooltip="{i18n>precessNew}"
				visible="{ButtonPropertyModel>/duplicateCheckProcessBtnVisible}"/>
			<Button text="{i18n>closeBtn}" press="CloseDuplicateCheckDialog" tooltip="{i18n>closeBtn}"
				visible="{ButtonPropertyModel>/duplicateCheckCloseBtnVisible}"/>
		</Toolbar>
		<!--<beginButton>-->
		<!--	<Button text="{i18n>Change}" press="HandleChangeCustomerDuplicate" tooltip="{i18n>Change}"/>-->
		<!--</beginButton>-->
		<!--<endButton>-->
		<!--	<Button text="{i18n>precessNew}" press="HandleNewustomerDuplicate" tooltip="{i18n>precessNew}"/>-->
		<!--</endButton>-->
	</Dialog>
</core:FragmentDefinition>