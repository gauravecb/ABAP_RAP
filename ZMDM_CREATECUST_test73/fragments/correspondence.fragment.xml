<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="{PanelModelSet>/CorrespondencePanel}">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>Correspondence}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<f:SimpleForm class="customClassTitle editableForm" editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8"
			labelSpanS="6" adjustLabelSpan="false" emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
			<f:content>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>DunnProcedure}" wrapping="true"
					required="{path:'RequiredOptionalSet>/DunnProcCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox change="onLivechangeofDunnProcedure" items="{NavDunnProcedureSet>/results}"
					enabled="{path:'RequiredOptionalSet>/DunnProcCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					valueStateText="{AccountingDataModelSet>/Correspondence/DunnProcedureMessage}"
					valueState="{AccountingDataModelSet>/Correspondence/DunnProcedureState}"
						tooltip="{AccountingDataModelSet>/Correspondence/DunnProcedureTooltip}"
					selectedKey="{AccountingDataModelSet>/Correspondence/DunnProcedure}" placeholder="{i18n>select} {i18n>DunnProcedure}">
					<core:Item key="{NavDunnProcedureSet>Mahna}"
						text="{parts:[{path:'NavDunnProcedureSet>Mahna'},{path:'NavDunnProcedureSet>Textm'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>LastDunned}" wrapping="true"
					required="{path:'RequiredOptionalSet>/LastDunnCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
			<!--	<Input
				placeholder="DD.MM.YYYY"
				maxLength="10"
				valueStateText="{AccountingDataModelSet>/Correspondence/LastDunnedMessage}"
					valueState="{AccountingDataModelSet>/Correspondence/LastDunnedState}"
					enabled="{parts:[{path:'RequiredOptionalSet>/LastDunnCond'},{path:'AccountingDataModelSet>/Correspondence/DunnProcedure'}],formatter:'MDMZCUSTOMER.model.Formatter.basedOnDunningProcedure'}"
					liveChange="onLiveChangeofLastDunned" value="{AccountingDataModelSet>/Correspondence/LastDunned}"/>-->
					<DatePicker
                displayFormat="dd.MM.YYYY"
                valueStateText="{AccountingDataModelSet>/Correspondence/LastDunnedMessage}"
                    valueState="{AccountingDataModelSet>/Correspondence/LastDunnedState}"
                    tooltip="{AccountingDataModelSet>/Correspondence/LastDunnedTooltip}"
                    enabled="{parts:[{path:'RequiredOptionalSet>/LastDunnCond'},{path:'AccountingDataModelSet>/Correspondence/DunnProcedure'}],formatter:'MDMZCUSTOMER.model.Formatter.basedOnDunningProcedure'}"
                    change="onLiveChangeofLastDunned" dateValue="{AccountingDataModelSet>/Correspondence/LastDunned}"/>
				<Label textAlign="Left" text="{i18n>DunningClerk}" wrapping="true"
					required="{path:'RequiredOptionalSet>/DunningClerkCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox enabled="{parts:[{path:'RequiredOptionalSet>/DunningClerkCond'},{path:'AccountingDataModelSet>/Correspondence/DunnProcedure'}],formatter:'MDMZCUSTOMER.model.Formatter.basedOnDunningProcedure'}"
					change="onLivechangeofDunningClerk" items="{NavDunningClerkSet>/results}"
					valueStateText="{AccountingDataModelSet>/Correspondence/DunningClerkMessage}"
						tooltip="{AccountingDataModelSet>/Correspondence/DunningClerkTooltip}"
					valueState="{AccountingDataModelSet>/Correspondence/DunningClerkState}" selectedKey="{AccountingDataModelSet>/Correspondence/DunningClerk}"
					placeholder="{i18n>select} {i18n>DunningClerk}">
					<core:Item key="{NavDunningClerkSet>Dunning_Area}"
						text="{parts:[{path:'NavDunningClerkSet>Dunning_Area'},{path:'NavDunningClerkSet>Text'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>DunningBlock}" wrapping="true"
					required="{path:'RequiredOptionalSet>/DunnigBlockCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox enabled="{parts:[{path:'RequiredOptionalSet>/DunnigBlockCond'},{path:'AccountingDataModelSet>/Correspondence/DunnProcedure'}],formatter:'MDMZCUSTOMER.model.Formatter.basedOnDunningProcedure'}"
					change="onLivechangeofDunningBlock" items="{NavDunningBlockSet>/results}"
						tooltip="{AccountingDataModelSet>/Correspondence/DunningBlockTooltip}"
					valueStateText="{AccountingDataModelSet>/Correspondence/DunningBlockMessage}"
					valueState="{AccountingDataModelSet>/Correspondence/DunningBlockState}" selectedKey="{AccountingDataModelSet>/Correspondence/DunningBlock}"
					placeholder="{i18n>select} {i18n>DunningBlock}">
					<core:Item key="{NavDunningBlockSet>Mansp}"
						text="{parts:[{path:'NavDunningBlockSet>Mansp'},{path:'NavDunningBlockSet>Text1'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				</ComboBox>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>DunningLevel}" wrapping="true"
					required="{path:'RequiredOptionalSet>/DunLevelCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<Input valueStateText="{AccountingDataModelSet>/Correspondence/DunningLevelMessage}"
					valueState="{AccountingDataModelSet>/Correspondence/DunningLevelState}"
					tooltip="{AccountingDataModelSet>/Correspondence/DunningLevelTooltip}"
					enabled="{parts:[{path:'RequiredOptionalSet>/DunLevelCond'},{path:'AccountingDataModelSet>/Correspondence/DunnProcedure'}],formatter:'MDMZCUSTOMER.model.Formatter.basedOnDunningProcedure'}"
					liveChange="onLiveChangeofDunningLevel" value="{AccountingDataModelSet>/Correspondence/DunningLevel}"/>
				<Label textAlign="Left" text="{i18n>DunningArea}" wrapping="true"
					required="{path:'RequiredOptionalSet>/DunnAreaCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox enabled="{parts:[{path:'RequiredOptionalSet>/DunnAreaCond'},{path:'AccountingDataModelSet>/Correspondence/DunnProcedure'}],formatter:'MDMZCUSTOMER.model.Formatter.basedOnDunningProcedure'}"
					change="onLivechangeofDunningArea" items="{NavDunningAreaSet>/results}"
					tooltip="{AccountingDataModelSet>/Correspondence/DunningAreaTooltip}"
					valueStateText="{AccountingDataModelSet>/Correspondence/DunningAreaMessage}"
					valueState="{AccountingDataModelSet>/Correspondence/DunningAreaState}" selectedKey="{AccountingDataModelSet>/Correspondence/DunningArea}"
					placeholder="{i18n>select} {i18n>DunningArea}">
					<core:Item key="{NavDunningAreaSet>Accounting_Clerk}"
						text="{parts:[{path:'NavDunningAreaSet>Accounting_Clerk'},{path:'NavDunningAreaSet>Name_of_Accounting_Clerk'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>ClerkAbbrev}" wrapping="true"
					required="{path:'RequiredOptionalSet>/ClerkAbbrvCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox enabled="{path:'RequiredOptionalSet>/ClerkAbbrvCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					change="onLivechangeofClerkAbbrev" items="{NavClerkAbbrevSet>/results}"
						tooltip="{AccountingDataModelSet>/Correspondence/ClerkAbbrevTooltip}"
					valueStateText="{AccountingDataModelSet>/Correspondence/ClerkAbbrevMessage}"
					valueState="{AccountingDataModelSet>/Correspondence/ClerkAbbrevState}" selectedKey="{AccountingDataModelSet>/Correspondence/ClerkAbbrev}"
					placeholder="{i18n>select} {i18n>ClerkAbbrev}">
					<core:Item key="{NavClerkAbbrevSet>Clerk_Abbreviation}"
						text="{parts:[{path:'NavClerkAbbrevSet>Clerk_Abbreviation'},{path:'NavClerkAbbrevSet>Name'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				</ComboBox>
				<Label textAlign="Left" text="{i18n>AcctAtCust}" wrapping="true"
					required="{path:'RequiredOptionalSet>/AccAtCustCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<Input valueStateText="{AccountingDataModelSet>/Correspondence/AcctAtCustMessage}"
				tooltip="{AccountingDataModelSet>/Correspondence/AcctAtCustTooltip}"
					valueState="{AccountingDataModelSet>/Correspondence/AcctAtCustState}"
					enabled="{path:'RequiredOptionalSet>/AccAtCustCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					maxLength="12"
					liveChange="onLiveChangeofAcctAtCust" value="{AccountingDataModelSet>/Correspondence/AcctAtCust}"/>
				<core:Title/>
				<Label textAlign="Left" text="{i18n>CustomerUser}" wrapping="true"
					required="{path:'RequiredOptionalSet>/CustomUsrCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<Input valueStateText="{AccountingDataModelSet>/Correspondence/CustomerUserMessage}"
					valueState="{AccountingDataModelSet>/Correspondence/CustomerUserState}"
					maxLength="15"
						tooltip="{AccountingDataModelSet>/Correspondence/CustomerUserTooltip}"
					enabled="{path:'RequiredOptionalSet>/CustomUsrCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					liveChange="onLiveChangeofCustomerUser" value="{AccountingDataModelSet>/Correspondence/CustomerUser}"/>
				<Label textAlign="Left" text="{i18n>Clerk'sInternet}" wrapping="true"
					required="{path:'RequiredOptionalSet>/ClerkIntrnCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<Input maxLength="130" valueStateText="{AccountingDataModelSet>/Correspondence/ClerksInternetMessage}"
					valueState="{AccountingDataModelSet>/Correspondence/ClerksInternetState}"
					placeholder="Ex: abc@gmail.com"
						tooltip="{AccountingDataModelSet>/Correspondence/ClerksInternetTooltip}"
					enabled="{path:'RequiredOptionalSet>/ClerkIntrnCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					change="onLiveChangeofClerksInternet" value="{AccountingDataModelSet>/Correspondence/ClerksInternet}"/>
				<Label textAlign="Left" text="{i18n>AccountStatement}" wrapping="true"
					required="{path:'RequiredOptionalSet>/AccStmntCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<ComboBox enabled="{path:'RequiredOptionalSet>/AccStmntCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
					change="onLivechangeofAccountStatement" items="{NavAccountStatementSet>/results}"
					valueStateText="{AccountingDataModelSet>/Correspondence/AccountStatementMessage}"
						tooltip="{AccountingDataModelSet>/Correspondence/AccountStatementTooltip}"
					valueState="{AccountingDataModelSet>/Correspondence/AccountStatementState}"
					selectedKey="{AccountingDataModelSet>/Correspondence/AccountStatement}" placeholder="{i18n>select} {i18n>AccountStatement}">
					<core:Item key="{NavAccountStatementSet>Xausz}"
						text="{parts:[{path:'NavAccountStatementSet>Xausz'},{path:'NavAccountStatementSet>Text1'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
				</ComboBox>
			</f:content>
		</f:SimpleForm>
	</Panel>
</core:FragmentDefinition>