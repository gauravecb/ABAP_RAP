<core:FragmentDefinition xmlns="sap.m" xmlns:u="sap.ui.table" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<Dialog title="{i18n>detailsSubpageHeading}" contentWidth="75%" class="sapUiSizeCompact">
		<content>
			<Table items="{subHyperLinkModelSet>/results}" select = "onPressSubCompListTable"  enableBusyIndicator="false" mode="SingleSelectLeft">
				<headerToolbar>
					<OverflowToolbar>
						<Title text="Results ({subHyperLinkModelSet>/results/length})"/>
					</OverflowToolbar>
				</headerToolbar>
				<columns>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Label text="{i18n>compCodeSubPage}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Label text="{i18n>compCodeDelFlagSubPage}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Label text="{i18n>salesOrgsubPage}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Label text="{i18n>dustributionChannlSubPage}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Label text="{i18n>divisionSubPage}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Label text="{i18n>salesAreaDeletionFlag}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<Text text="{subHyperLinkModelSet>Companycode}"></Text>
						<Text text="{subHyperLinkModelSet>Cocodedeletion}"></Text>
						<Text text="{subHyperLinkModelSet>Salesorg}"></Text>
						<Text text="{subHyperLinkModelSet>Distrchannel}"></Text>
						<Text text="{subHyperLinkModelSet>Division}"></Text>
						<Text text="{subHyperLinkModelSet>Salesareadeletion}"></Text>
					</ColumnListItem>
				</items>
			</Table>
		</content>
		<buttons>
		<Button text="{i18n>Change}" press="onPressChangeCustomer" 
				visible="{path:'OpenEditModelSet>/EditOrOpen',formatter:'MDMZCUSTOMER.model.Formatter.buttonHanldeiExBusSubEdit'}"
			tooltip="{i18n>Change}"/>
			<Button text="{i18n>extenstionsubPage}"
			visible="{path:'OpenEditModelSet>/EditOrOpen',formatter:'MDMZCUSTOMER.model.Formatter.buttonHanldeiExBusSubEdit'}"
			press="onPressExtenstion" tooltip="{i18n>extenstionsubPage}"/>
			<Button text="{i18n>Open}" 
			visible="{path:'OpenEditModelSet>/EditOrOpen',formatter:'MDMZCUSTOMER.model.Formatter.buttonHanldeiExBusSubOpen'}"
			press="onPressOpenDialogButton" tooltip="{i18n>Open}"/>
			<Button text="{i18n>Close}" press="onPressClose" tooltip="{i18n>Close}"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>