<core:FragmentDefinition xmlns="sap.m" xmlns:u="sap.ui.table" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<Dialog title="{i18n>ibanBankDetailsFrgTitle}" contentWidth="75%" class="sapUiSizeCompact">
		<content>
			<Table enableBusyIndicator="false" select="onPressOkBankKeySelection" items="{ModelBankDetailsMoreSet>/results}"  mode="SingleSelectMaster">
				<columns>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>countryBank}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>bankKey}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>nameOfBank}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>HouseNumberAndStreet}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>cityBank}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>bankBranch}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>swiftBic}"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="{i18n>bankNumber}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<Text text="{ModelBankDetailsMoreSet>BankCountry}"></Text>
						<Text text="{ModelBankDetailsMoreSet>BankKey}"></Text>
						<Text text="{ModelBankDetailsMoreSet>NameOfBank}"></Text>
						<Text text="{ModelBankDetailsMoreSet>HouseNumber}"></Text>
						<Text text="{ModelBankDetailsMoreSet>City}"></Text>
						<Text text="{ModelBankDetailsMoreSet>BankBranch}"></Text>
						<Text text="{ModelBankDetailsMoreSet>Swiftbic}"></Text>
						<Text text="{ModelBankDetailsMoreSet>BankNumber}"></Text>
					</ColumnListItem>
				</items>
			</Table>
		</content>
		<beginButton>
		<!--	<Button text="{i18n>OK}" press="onPressOkBankKeySelection" tooltip="{i18n>OK}"/>-->
		</beginButton>
		<endButton>
			<Button text="{i18n>Cancel}" press="onPressCancelBankKeySelection" tooltip="{i18n>Cancel}"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>