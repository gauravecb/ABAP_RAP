<core:FragmentDefinition xmlns="sap.m" xmlns:u="sap.ui.table" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<Dialog title="{i18n>duplicateCheckHeadingComment}" class="sapUiSizeCompact">
		<content>
			<f:SimpleForm maxContainerCols="1" editable="true" layout="ResponsiveGridLayout" labelSpanL="12" labelSpanM="12" emptySpanL="0"
				emptySpanM="0" columnsL="1" columnsM="1" class="editableForm simplFormPaddCls">
				<f:content>
					<Label text="{i18n>duplicateChecklblName}"></Label>
					<TextArea rows="5" value="{CommentModelSet>/Comment}" valueState="{CommentModelSet>/CommentMsgState}"
						liveChange="liveChangeDuplicateCheckComment" valueStateText="{i18n>pleaseEnterValidReasonFo}"></TextArea>
				</f:content>
			</f:SimpleForm>
		</content>
		<beginButton>
			<Button text="{i18n>OK}" press="HandleOkCustomerDuplicate" tooltip="{i18n>Ok}"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>Cancel}" press="HandleCancelDuplicate" tooltip="{i18n>Cancel}"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>