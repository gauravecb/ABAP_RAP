<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="{PanelModelSet>/CustDataUnloadingPointsPanel}">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>UnloadingPoints}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<VBox items="{CustomerDataSet>/UnloadingPoint/UnloadPointArr}">
			<VBox visible="{CustomerDataSet>VisibleBasedOnNext}">
				<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8" labelSpanS="6" adjustLabelSpan="false"
					emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
					<f:content>
						<core:Title/>
						<Label textAlign="Left" required="{path:'RequiredOptionalSet>/UnlodPointCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"
							text="{i18n>UnloadPoint}" wrapping="true"/>
						<Input value="{CustomerDataSet>UnloadPoint}" valueStateText="{CustomerDataSet>UnloadPointMessage}"
							valueState="{CustomerDataSet>UnloadPointState}"
							enabled="{path:'RequiredOptionalSet>/UnlodPointCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
							liveChange="liveChangeUnloadingPoints" maxLength="25" tooltip="{CustomerDataSet>UnloadPointTooltip}"/>
						<Label textAlign="Left" required="{path:'RequiredOptionalSet>/GoodsRecHrsCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"
							text="{i18n>GoodsReceivingHours}" wrapping="true"/>
						<ComboBox valueStateText="{CustomerDataSet>GoodsRecHrsMessage}" valueState="{CustomerDataSet>GoodsRecHrsState}"
							items="{GoodsRecCDCombSet>/results}"
							enabled="{parts:[{path:'CustomerDataSet>UnloadPoint'},{path:'CustomerDataSet>CalenderKey'},{path:'RequiredOptionalSet>/GoodsRecHrsCond'}],formatter:'MDMZCUSTOMER.model.Formatter.enableBasedOnUnloadPointCalenderKey'}"
							selectedKey="{CustomerDataSet>GoodsRecHrs}" change="liveChangeGoodsRecivingHrs" tooltip="{CustomerDataSet>GoodsRecHrsTooltip}">
							<core:Item key="{GoodsRecCDCombSet>Wanid}"
								text="{parts:[{path:'GoodsRecCDCombSet>Wanid'},{path:'GoodsRecCDCombSet>Vtext'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
						</ComboBox>
						<core:Title/>
						<Label textAlign="Left" required="{path:'RequiredOptionalSet>/DefaultCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"
							text="{i18n>Default}" wrapping="true"/>
						<CheckBox width="10%" valueStateText="{CustomerDataSet>DefaultMessage}" valueState="{CustomerDataSet>DefaultState}"
							enabled="{path:'RequiredOptionalSet>/DefaultCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
							selected="{CustomerDataSet>Default}" select="onSelectDefaultCD"/>
						<core:Title/>
						<Label required="{path:'RequiredOptionalSet>/CalenderKeyCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}" textAlign="Left"
							text="{i18n>CalendarKey}" wrapping="true"/>
						<ComboBox items="{CalenderKeyCDCombSet>/results}" valueStateText="{CustomerDataSet>CalenderKeyMessage}"
							valueState="{CustomerDataSet>CalenderKeyState}"
							enabled="{path:'RequiredOptionalSet>/CalenderKeyCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
							change="onChnageCalenderKeyCD" selectedKey="{CustomerDataSet>CalenderKey}" tooltip="{CustomerDataSet>CalenderKeyTooltip}">
							<core:Item key="{CalenderKeyCDCombSet>Ident}"
								text="{parts:[{path:'CalenderKeyCDCombSet>Ident'},{path:'CalenderKeyCDCombSet>Ltext'}],formatter:'MDMZCUSTOMER.model.Formatter.codeValuePair'}"/>
						</ComboBox>
						<core:Title/>
					</f:content>
				</f:SimpleForm>
				<Table items="{CustomerDataSet>DayTable}">
					<columns>
						<Column>
							<Label text="{i18n>dayTable}"></Label>
						</Column>
						<Column>
							<Label text="{i18n>MorningOpenTable}"></Label>
						</Column>
						<Column>
							<Label text="{i18n>MorningCloseTable}"></Label>
						</Column>
						<Column>
							<Label text="{i18n>AfternoonOpenTable}"></Label>
						</Column>
						<Column>
							<Label text="{i18n>AfternoonCloseTable}"></Label>
						</Column>
					</columns>
					<items>
						<ColumnListItem>
							<Text text="{CustomerDataSet>Weekday}"></Text>
							<TimePicker dateValue="{CustomerDataSet>Morningopen}" valueStateText="{CustomerDataSet>MorningOpenMessage}"
								valueState="{CustomerDataSet>MorningOpenState}" valueFormat="HH:mm" displayFormat="HH:mm" change="onChangeMorningOpenTime"
								enabled="{parts:[{path:'CustomerDataSet>MornOpnEnable'},{path:'RequiredOptionalSet>/EditUnloadPoint'}],formatter:'MDMZCUSTOMER.model.Formatter.DayTimeTableEnabled'}"
								tooltip="{CustomerDataSet>MorningOpenTooltip}"/>
							<TimePicker valueStateText="{CustomerDataSet>MorningClosenMessage}" valueState="{CustomerDataSet>MorningCloseState}"
								dateValue="{CustomerDataSet>Morningclose}" valueFormat="HH:mm" displayFormat="HH:mm" change="onChangeMorningCloseTime"
								enabled="{parts:[{path:'CustomerDataSet>MornCloseEnable'},{path:'RequiredOptionalSet>/EditUnloadPoint'}],formatter:'MDMZCUSTOMER.model.Formatter.DayTimeTableEnabled'}"
								tooltip="{CustomerDataSet>MorningCloseTooltip}"/>
							<TimePicker valueStateText="{CustomerDataSet>AfternoonOpenMessage}" valueState="{CustomerDataSet>AfternoonOpenState}"
								dateValue="{CustomerDataSet>Afternoonopen}" valueFormat="HH:mm" displayFormat="HH:mm" change="onChangeAfternoonOpenTime"
								enabled="{parts:[{path:'CustomerDataSet>AfterOpnEnable'},{path:'RequiredOptionalSet>/EditUnloadPoint'}],formatter:'MDMZCUSTOMER.model.Formatter.DayTimeTableEnabled'}"
								tooltip="{CustomerDataSet>AfternoonOpenTooltip}"/>
							<TimePicker valueStateText="{CustomerDataSet>AfternoonCloseMessage}" valueState="{CustomerDataSet>AfternoonCloseState}"
								dateValue="{CustomerDataSet>Afternoonclose}" valueFormat="HH:mm" displayFormat="HH:mm" change="onChangeAfternoonCloseTime"
								enabled="{parts:[{path:'CustomerDataSet>AfterCloseEnable'},{path:'RequiredOptionalSet>/EditUnloadPoint'}],formatter:'MDMZCUSTOMER.model.Formatter.DayTimeTableEnabled'}"
								tooltip="{CustomerDataSet>AfternoonCloseTooltip}"/>
						</ColumnListItem>
					</items>
				</Table>
				<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8" labelSpanS="6" adjustLabelSpan="false"
					emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
					<f:content>
						<core:Title/>
						<core:Title/>
						<core:Title/>
						<Label></Label>
						<Text text="{CustomerDataSet>TotalCount}"></Text>
						<Button text="+" tooltip="{i18n>addUnloadingPoints}" press="addNewUnloadingPointsCustomerDetails" enabled="{RequiredOptionalSet>/UnlodEdit}"></Button>
						<Button text="-" tooltip="{i18n>removeUnloadingPoints}" press="deleteUnloadingPointsForCustomerDetails"
							enabled="{RequiredOptionalSet>/UnlodEdit}"></Button>
						<Label></Label>
						<Button text="{i18n>PriviousBtn}" tooltip="{i18n>PriviousBtn}" press="priviosUnloadingPointsCustData" icon="sap-icon://slim-arrow-left"></Button>
						<Button text="{i18n>NextBtn}" iconFirst="false" tooltip="{i18n>NextBtn}" press="nextUnloadingPointsCustData"
							icon="sap-icon://slim-arrow-right"></Button>
						<core:Title/>
					</f:content>
				</f:SimpleForm>
			</VBox>
		</VBox>
	</Panel>
</core:FragmentDefinition>