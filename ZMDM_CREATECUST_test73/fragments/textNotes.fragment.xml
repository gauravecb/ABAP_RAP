	<VBox>
		<Panel expandable="true" expanded="{PanelModelSet>/TextNotesPanel}">
			<headerToolbar>
				<OverflowToolbar>
					<Label text="{i18n>TextNotes}" design="Bold"/>
				</OverflowToolbar>
			</headerToolbar>
			<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8" labelSpanS="6" adjustLabelSpan="false"
				emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
				<f:content>
					<Table enableBusyIndicator="false" items="{TextNotesModelSet>/TextNotes/TextNotes2}">
						<columns>
							<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
								<Label text="{i18n>Level}" required="{path:'RequiredOptionalSet>/LevelTNCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
								<Label text="{i18n>Language}" required="{path:'RequiredOptionalSet>/LanguageTNCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
								<Label text="{i18n>TextType}" required="{path:'RequiredOptionalSet>/TextTypeTNCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet" width="40%">
								<Label text="{i18n>FreeText}" required="{path:'RequiredOptionalSet>/FreeTextTNCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
								<Label text=""/>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<ComboBox enabled="{path:'RequiredOptionalSet>/LevelTNCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
									items="{textNotesLevelSet>/results}" change="onChnageLevelTextNotes" selectedKey="{TextNotesModelSet>Level}"
									placeholder="{i18n>select} {i18n>Level}" valueState="{TextNotesModelSet>LevelState}" valueStateText="{TextNotesModelSet>LevelMessage}"
									tooltip="{TextNotesModelSet>LevelTooltip}">
									<core:Item key="{textNotesLevelSet>Level_Text}" text="{textNotesLevelSet>Level_Text}"/>
								</ComboBox>
								<ComboBox enabled="{path:'RequiredOptionalSet>/LanguageTNCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
									items="{languageSet>/results}" change="onChangeLanguageTextNotes" selectedKey="{TextNotesModelSet>Language}"
									placeholder="{i18n>select} {i18n>Language}" valueState="{TextNotesModelSet>LanguageState}"
									valueStateText="{TextNotesModelSet>LanguageMessage}" tooltip="{TextNotesModelSet>LanguageTooltip}">
									<core:Item key="{languageSet>Language}" text="{languageSet>LanguageText}"/>
								</ComboBox>
								<ComboBox enabled="{path:'RequiredOptionalSet>/TextTypeTNCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
									items="{TextNotesModelSet>TextTypeArray}" change="onChangeTextTypeTextNotes" selectedKey="{TextNotesModelSet>TextType}"
									placeholder="{i18n>select} {i18n>TextType}" valueState="{TextNotesModelSet>TextTypeState}"
									valueStateText="{TextNotesModelSet>TextTypeMessage}" tooltip="{TextNotesModelSet>TextTypeTooltip}">
									<core:Item key="{TextNotesModelSet>Type}" text="{TextNotesModelSet>Type}"/>
								</ComboBox>
								<Input enabled="{path:'RequiredOptionalSet>/FreeTextTNCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}" maxLength="1000"
									change="onPressTextLengthInput" value="{TextNotesModelSet>FreeText}" valueState="{TextNotesModelSet>FreeTextState}"
									valueStateText="{TextNotesModelSet>FreeTextMessage}" tooltip="{TextNotesModelSet>FreeTextTooltip}"></Input>
								<Button
									enabled="{path:'TextNotesModelSet>FreeText',formatter:'MDMZCUSTOMER.model.Formatter.basedOnInputLengthNeedToShowcommentButtoninTextNotes'}"
									text="{i18n>DisplayFreeText}" press="onPressAddPopupTextNotes" width="100%" type="Accept"/>
							</ColumnListItem>
						</items>
					</Table>
				</f:content>
			</f:SimpleForm>
			<f:SimpleForm editable="true" layout="ResponsiveGridLayout">
				<f:content>
					<Toolbar>
						<ToolbarSpacer></ToolbarSpacer>
						<Button text="+" press="onPressAddTextNotes" width="5%" type="Accept" enabled="{RequiredOptionalSet>/TextNoteSectionEdit}"/>
					</Toolbar>
				</f:content>
			</f:SimpleForm>
		</Panel>
		<Panel expandable="true" expanded="false">
			<headerToolbar>
				<OverflowToolbar>
					<Label text="{i18n>Classification}" design="Bold"/>
				</OverflowToolbar>
			</headerToolbar>
			<f:SimpleForm editable="true" layout="ResponsiveGridLayout">
				<f:content>
					<Table enableBusyIndicator="false" items="{TextNotesModelSet>/Classification/Classification2}">
						<columns>
							<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
								<Label text="{i18n>ClassificationType}"
									required="{path:'RequiredOptionalSet>/ClassificationTypeTNCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet" width="40%">
								<Label text="{i18n>Classification}"
									required="{path:'RequiredOptionalSet>/ClassificationTNCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet" width="50%">
								<Label text="{i18n>Value}" required="{path:'RequiredOptionalSet>/ValueTNCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet" width="30%">
								<Label text=""/>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<ComboBox enabled="{path:'RequiredOptionalSet>/ClassificationTypeTNCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
									items="{classificationTypeSet>/results}" change="onChangeclassificationTypeSet" selectedKey="{TextNotesModelSet>ClassificationType}"
									placeholder="{i18n>select} {i18n>ClassificationType}" valueState="{TextNotesModelSet>ClassificationTypeState}"
									valueStateText="{TextNotesModelSet>ClassificationTypeMessage}" tooltip="{TextNotesModelSet>ClassificationTypeTooltip}">
									<core:Item key="{classificationTypeSet>ClassificationType}"
										text="{classificationTypeSet>ClassificationType}-{classificationTypeSet>Class_Text}"/>
								</ComboBox>
								<ComboBox enabled="{path:'RequiredOptionalSet>/ClassificationTNCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
									items="{TextNotesModelSet>ClassiFicationArray}" selectedKey="{TextNotesModelSet>Classification}"
									placeholder="{i18n>select} {i18n>Classification}" valueState="{TextNotesModelSet>ClassificationState}"
									valueStateText="{TextNotesModelSet>ClassificationMessage}" change="chnageClassifcationMessage"
									tooltip="{TextNotesModelSet>ClassificationTooltip}">
									<core:Item key="{TextNotesModelSet>Classification_Text}" text="{TextNotesModelSet>Classification_Text}"/>
								</ComboBox>
								<ComboBox enabled="{path:'RequiredOptionalSet>/ValueTNCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
									items="{TextNotesModelSet>ValueArray}" selectedKey="{TextNotesModelSet>Value}" valueState="{TextNotesModelSet>ValueState}"
									valueStateText="{TextNotesModelSet>ValueMessage}" change="chnageClassifcationValue" tooltip="{TextNotesModelSet>ValueTooltip}">
									<core:Item key="{TextNotesModelSet>Value_Num}" text="{TextNotesModelSet>Value_text}"/>
								</ComboBox>
							</ColumnListItem>
						</items>
					</Table>
				</f:content>
			</f:SimpleForm>
		</Panel>
	</VBox>
</core:FragmentDefinition>