<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<Panel expandable="true" expanded="{PanelModelSet>/BusPartDataExtIdntPanel}">
		<headerToolbar>
			<OverflowToolbar>
				<Label text="{i18n>ExternalIdentifier}" design="Bold"/>
			</OverflowToolbar>
		</headerToolbar>
		<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanL="5" labelSpanM="8" labelSpanS="6" adjustLabelSpan="false"
			emptySpanL="0" emptySpanM="0" columnsL="3" columnsM="3">
			<f:content>
				<core:Title/>
				<Label textAlign="Left" class="newclass" text="{i18n>ChamberofCommerce}" wrapping="true"
					required="{path:'RequiredOptionalSet>/ChamberOfCommCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"
				/>
				<Input maxLength="128"
					enabled="{path:'RequiredOptionalSet>/ChamberOfCommCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
				valueStateText="{CreateModelSet>/ExternalIdentifer/ChamberofCommerceMessage}"
			
					valueState="{CreateModelSet>/ExternalIdentifer/ChamberofCommerceState}" value="{CreateModelSet>/ExternalIdentifer/ChamberOf_Commerce}"
					liveChange="liveChangeChamberOfCommerceExternIden" tooltip="{CreateModelSet>/ExternalIdentifer/ChamberofCommerceTooltip}"></Input>
			<!--	<Label text="{i18n>ExternalIdentifierType}"/>
				<ComboBox items="{externalIdentifierSet>/ExtIdentifier}" change="liveChangeExternalIdentTypeView"
					selectedKey="{CreateModelSet>/ExternalIdentifer/ExternalIdentifier_Type}" placeholder="{i18n>select} {i18n>ExternalIdentType}"
					valueState="{CreateModelSet>/ExternalIdentifer/ExternalTypeStateView}" valueStateText="{CreateModelSet>/ExternalIdentifer/ExternalTypeMessageView}">
					<core:Item key="{externalIdentifierSet>type}" text="{externalIdentifierSet>type}"/>
				</ComboBox>-->
				<core:Title/>
				<Label textAlign="Left" class="newclass" text="{i18n>DUNS}" 	required="{path:'RequiredOptionalSet>/DunsCond',formatter:'MDMZCUSTOMER.model.Formatter.requiredLabel'}"/>
				<Input maxLength="128"
					enabled="{path:'RequiredOptionalSet>/DunsCond',formatter:'MDMZCUSTOMER.model.Formatter.optionalDisabled'}"
				valueStateText="{CreateModelSet>/ExternalIdentifer/DUNSMessage}" valueState="{CreateModelSet>/ExternalIdentifer/DUNSState}"
					value="{CreateModelSet>/ExternalIdentifer/DUNS}" liveChange="liveChangeDunsExternIdent" tooltip="{CreateModelSet>/ExternalIdentifer/DUNSTooltip}"></Input>
				<!--<Label text="{i18n>ExternalIdentifierValue}"/>
				<Input visible="false" value="{CreateModelSet>/ExternalIdentifer/ExternalIdentifier_Value}" liveChange="liveChangeExternalIdentTypeValueView"
					valueState="{CreateModelSet>/ExternalIdentifer/ExtValueStateView}" valueStateText="{CreateModelSet>/ExternalIdentifer/ExtValueMessageView}"></Input>
					-->
					
					
				<core:Title/>
				<Label></Label>
				<Button text="+" press="onPressAddExternalIdentifierFrag" tooltip="{i18n>clickhereforAdditionalIdentifiers}"></Button>
				<Label></Label>
				<Link press="onPressAddExternalIdentifierFrag" text="{parts:[{path:'CreateModelSet>/ExternalIdentifer/TotalExternalIdent'},{path:'i18n>labelCountExternIdent'}],formatter:'MDMZCUSTOMER.model.Formatter.countBasedExtTypeValue'}"></Link>
				<!--<Label></Label>
				<Button text="{i18n>validate}" tooltip="{i18n>validate}" press="externalIdentifierValidation"/>-->
			</f:content>
		</f:SimpleForm>
	</Panel>
</core:FragmentDefinition>