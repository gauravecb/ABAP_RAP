			<pages>
				<Page id="page" title="{i18n>title}">
				<content>
				    <List noDataText="Drop list items here" id="list0" xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" items="{/BusinessPartnerSet}">
