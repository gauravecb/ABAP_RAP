{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run KumarFirstFoundry-BusinessParameter",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/kumarfirstfoundry-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsBusinessParameter/index.html",
        "--ui5",
        "{\"version\":\"1.78.10\"}"
      ],
      "cwd": "${workspaceFolder}/BusinessParameter",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/KumarFirstFoundry/BusinessParameter\"}"
      },
      "envFile": "${workspaceFolder}/BusinessParameter/.env1",
      "preLaunchTask": "Build KumarFirstFoundry"
    }
  ]
}